<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="seq_prescriptions_practice" model="ir.sequence">
        <field name="name">Practice sequence</field>
        <field name="code">prescriptions.practice</field>
        <field name="prefix">MP</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_prescriptions_location" model="ir.sequence">
        <field name="name">Location sequence</field>
        <field name="code">prescriptions.location</field>
        <field name="prefix">ML</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_prescriptions_practitioner" model="ir.sequence">
        <field name="name">Practitioner sequence</field>
        <field name="code">prescriptions.practitioner</field>
        <field name="prefix">PT</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_prescriptions_patient" model="ir.sequence">
        <field name="name">Patient sequence</field>
        <field name="code">prescriptions.patient</field>
        <field name="prefix">PID</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_prescriptions_flag" model="ir.sequence">
        <field name="name">Flag sequence</field>
        <field name="code">prescriptions.flag</field>
        <field name="prefix">FL</field>
        <field name="padding">6</field>
    </record>
</odoo>
