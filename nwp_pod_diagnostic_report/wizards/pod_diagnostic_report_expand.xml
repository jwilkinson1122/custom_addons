<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="pod_diagnostic_report_expand_form_view">
        <field
            name="name"
        >pod.diagnostic.report.expand.form (in pod_diagnostic_report)</field>
        <field name="model">pod.diagnostic.report.expand</field>
        <field
            name="inherit_id"
            ref="pod_diagnostic_report.pod_diagnostic_report_expand_form_view"
        />
        <field name="arch" type="xml">
            <field name="template_id" position="attributes">
                <attribute
                    name="domain"
                >[('id','not in',template_ids),('report_category_id','=',report_category_id)]</attribute>
            </field>
            <field name="template_id" position="after">
                <field name="report_category_id" invisible="1" />
            </field>
        </field>
    </record>
</odoo>
