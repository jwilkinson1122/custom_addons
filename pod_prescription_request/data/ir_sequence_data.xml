<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="seq_pod_prescription_request" model="ir.sequence">
        <field name="name">Poditary prescription request sequence</field>
        <field name="code">pod.prescription.request</field>
        <field name="prefix">REQ</field>
        <field name="padding">8</field>
    </record>
    <record id="seq_pod_prescription_administration" model="ir.sequence">
        <field name="name">Podiatry prescription administration sequence</field>
        <field name="code">pod.prescription.administration</field>
        <field name="prefix">RX</field>
        <field name="padding">8</field>
    </record>
</odoo>
