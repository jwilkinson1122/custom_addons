<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="imaging_sequence" model="ir.sequence">
            <field name="name">Imaging Request</field>
            <field name="code">imaging_seq</field>
            <field name="prefix">IMG</field>
            <field name="padding">3</field>
        </record>

        <record id="podiatry_health_service_code_sequence" model="ir.sequence">
            <field name="name">Product</field>
            <field name="code">podiatry.health_service</field>
            <field name="prefix">SERV%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="prescription_order_sequence" model="ir.sequence">
            <field name="name">Prescriptions</field>
            <field name="code">podiatry.prescription.order</field>
            <field name="prefix">RX%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="podiatry_rx_sequence" model="ir.sequence">
            <field name="name">Podiatry Rx</field>
            <field name="code">pod_rx_seq</field>
            <field name="prefix">RX</field>
            <field name="padding">3</field>
        </record>

        <record id="rx_sequence" model="ir.sequence">
            <field name="name">Rx</field>
            <field name="code">rx_seq</field>
            <field name="prefix">RX</field>
            <field name="padding">3</field>
        </record>

        <record id="podiatry_health_service_sequence" model="ir.sequence">
            <field name="name">Podiatry Health Product</field>
            <field name="code">podiatry_health_ser</field>
            <field name="prefix">PODSR</field>
            <field name="padding">3</field>
        </record>

        <record id="prescription_invoice_sequence" model="ir.sequence">
            <field name="name">Prescription Invoice</field>
            <field name="code">pres_inv_seq</field>
            <field name="prefix">RX/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="rx_invoice_sequence" model="ir.sequence">
            <field name="name">Rx Invoice</field>
            <field name="code">rx_inv_seq</field>
            <field name="prefix">RX/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="podiatry_health_service_invoice_sequence" model="ir.sequence">
            <field name="name">Podiatry Health Product Invoice</field>
            <field name="code">podiatry_health_service_inv_seq</field>
            <field name="prefix">POD/HS/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="podiatry_rx_invoice_sequence" model="ir.sequence">
            <field name="name">Podiatry Rx Invoice</field>
            <field name="code">podiatry_rx_inv_seq</field>
            <field name="prefix">POD/RX/INV/</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
