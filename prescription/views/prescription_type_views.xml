<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id='prescription_type_view_form' model='ir.ui.view'>
        <field name="name">prescription.type.form</field>
        <field name="model">prescription.type</field>
        <field name="arch" type="xml">
            <form string="Prescription" js_class="prescription_form">
                <field name="service_activity" invisible="1"/>
                <header>
                    <!-- <button string="Apply New Practitioner" class="btn btn-primary" type="object" name="action_accept_practitioner_change" invisible="not practitioner_id"/> -->
                    <field name="state_id" widget="statusbar" options="{'clickable': '1'}"/>
                </header>
                <sheet>
                    <field name="company_id" invisible="1"/>
                    <field name="currency_id" invisible="1"/>
                    <field name="country_code" invisible="1"/>
                    <div class="oe_button_box" name="button_box">
                        <button name="open_assignation_logs" type="object" class="oe_stat_button" icon="fa-history">
                            <field name="history_count" widget="statinfo" string="Practitioners History"/>
                        </button>
                        <button name="return_action_to_open" type="object" class="oe_stat_button" icon="fa-book" context="{'xml_id':'prescription_type_log_action', 'search_default_inactive': not active}" help="show the prescription for this prescription">
                            <field name="prescription_count" widget="statinfo" string="Prescription"/>
                        </button>
                        <button name="return_action_to_open" type="object" class="oe_stat_button" icon="fa-wrench" context="{'xml_id':'prescription_type_log_services_action', 'search_default_inactive': not active}" invisible="service_activity != 'none'" help="show the services logs for this prescription">
                            <field name="service_count" widget="statinfo" string="Adjustments"/>
                        </button>
                        <button name="return_action_to_open" type="object" class="oe_stat_button text-danger" icon="fa-wrench" context="{'xml_id':'prescription_type_log_services_action', 'search_default_inactive': not active}" invisible="service_activity != 'overdue'" help="show the services logs for this prescription">
                            <field name="service_count" widget="statinfo" string="Adjustments"/>
                        </button>
                        <button name="return_action_to_open" type="object" class="oe_stat_button text-warning" icon="fa-wrench" context="{'xml_id':'prescription_type_log_services_action', 'search_default_inactive': not active}" invisible="service_activity != 'today'" help="show the services logs for this prescription">
                            <field name="service_count" widget="statinfo" string="Adjustments"/>
                        </button>
                        <button name="return_action_to_open" type="object" class="oe_stat_button" icon="fa-tachometer" context="{'xml_id':'prescription_type_measure_action'}" help="show the measure logs for this prescription" invisible="prescription_type != 'custom'">
                            <field name="measure_count" widget="statinfo" string="Measures"/>
                        </button>
                    </div>
                    <widget name="web_ribbon" title="Archived" bg_color="text-bg-danger" invisible="active"/>
                    <field name="image_128" widget='image' class="oe_avatar"/>
                    <div class="oe_title">
                        <label for="model_id"/>
                        <h1>
                            <field name="model_id" placeholder="e.g. Superglass" class="w-100"/>
                        </h1>
                        <label for="prescription_id"/>
                        <h2>
                            <field name="prescription_id" class="oe_inline" placeholder="e.g. PAE 326"/>
                        </h2>
                        <label for="tag_ids" class="me-3"/>
                        <field name="tag_ids" widget="many2many_tags" options="{'color_field': 'color', 'no_create_edit': True}"/>
                    </div>
                    <group col="2">
                        <group string="Practitioner">
                            <field name="active" invisible="1"/>
                            <field name="prescription_type" invisible="1"/>
                            <field name="account_id" domain="[('is_parent_account', '=', True)]" widget="many2one_avatar"/>
                            <field name="location_id" domain="[('is_location', '=', True)]" widget="many2one_avatar"/>
                            <!-- <field name="location_id" domain="[('is_location','=',True)]"/> -->
                            
                            <!-- <field name="location_id" domain="['|', ('company_id', '=', False ), ('company_id', '=', company_id)]" widget="many2one_avatar"/> -->
                            <field name="practitioner_id" domain="[('is_practitioner', '=', True), ('parent_id', '=', location_id)]" widget="many2one_avatar"/>
                            <field name="plan_to_change_prescription" groups="prescription.prescription_group_manager" invisible="prescription_type != 'custom'"/>
                            <field name="plan_to_change_otc" groups="prescription.prescription_group_manager" invisible="prescription_type != 'otc'"/>
                            <field name="next_assignation_date"/>
                            <field name="company_id" groups="base.group_multi_company"/>
                        </group>
                        <group string="Prescription">
                            <field name="category_id"/>
                            <field name="order_date"/>
                            <field name="received_date" invisible="prescription_type != 'custom'"/>
                            <field name="cancelled_date" invisible="prescription_type != 'custom'"/>
                            <field name="oin_sn"/>
                            <label for="measure" invisible="prescription_type != 'custom'"/>
                            <div class="o_row o_hr_narrow_field" invisible="prescription_type != 'custom'">
                                <field name="measure"/>
                                <field name="laterality"/>
                            </div>
                            <field name="manager_id" domain="[('share', '=', False)]" widget="many2one_avatar_user"/>
                            <field name="location"/>
                        </group>
                    </group>
                    <field name="prescription_properties" columns="2"/>
                    <notebook>
                        <page string="Tax Info" name="page_tax_info">
                            <group>
                                <group col="1" name="fiscality_group" string="Fiscality">
                                    <group col="1" name="fiscality_first_group">
                                        <field name="horsepower_tax" widget="monetary"/>
                                    </group>
                                </group>
                                <group string="Prescription">
                                    <field name="first_prescription_date"/>
                                    <field name="prescription_price" widget="monetary"/>
                                    <field name="net_prescription_price" widget="monetary"/>
                                    <field name="residual_value" widget="monetary"/>
                                </group>
                            </group>
                        </page>
                        <page string="Model" name="page_model">
                            <group>
                                <group string="Model">
                                    <field name="model_year"/>
                                    <field name="arch_height"/>
                                    <field name="color"/>
                                    <field name="qty"/>

                                    <field name="pairs"/>
                                    <field name="trailer_hook"/>
                                    <!-- <field name="shoe_type" invisible="prescription_type != 'otc'"/> -->
                                    <field name="shoe_type"/>
                                    <label for="shoe_size"/>
                                    <div class="o_row">
                                        <field name="shoe_size" />
                                        <!-- <span>cm</span> -->
                                    </div>
                                    <field name="electric_assistance"/>
                                </group>
                                <group string="Engine">
                                    <field name="horsepower"/>
                                    <label for="power"/>
                                    <div class="o_row">
                                        <field name="power"/>
                                        <span>kW</span>
                                    </div>
                                    <field name="shell_type"/>
                                    <label for="co2"/>
                                    <div class="o_row" name="co2">
                                        <field name="co2"/>
                                        <span>g/km</span>
                                    </div>
                                    <field name="co2_standard"/>
                                </group>
                            </group>
                        </page>
                        <page string="Note" name="note">
                            <field name="description" nolabel="1" placeholder="Write here any other information related to this prescription" />
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids"/>
                    <field name="activity_ids"/>
                    <field name="message_ids"/>
                </div>
            </form>
        </field>
    </record>

    <record id='prescription_type_view_tree' model='ir.ui.view'>
        <field name="name">prescription.type.tree</field>
        <field name="model">prescription.type</field>
        <field name="arch" type="xml">
            <tree string="Prescription" decoration-warning="prescription_renewal_due_soon and not prescription_renewal_overdue" decoration-danger="prescription_renewal_overdue" multi_edit="1" sample="1">
                <field name="active" column_invisible="True"/>
                <field name="prescription_id" readonly="1"/>
                <field name="model_id" widget="many2one_avatar" readonly="1"/>
                <field name="category_id"/>
                <field name="manager_id" optional="hide" widget="many2one_avatar_user"/>
                <field name="location_id" widget="many2one_avatar" readonly="1" optional="show"/>
                <field name="practitioner_id" widget="many2one_avatar" readonly="1" optional="show"/>
                <field name="log_practitioners" column_invisible="True"/>
                <field name="oin_sn" readonly="1" optional="hide"/>
                <field name="co2" string="CO2 Emissions g/km" optional="hide" readonly="1"/>
                <field name="received_date" readonly="1"/>
                <field name="tag_ids" widget="many2many_tags" options="{'color_field': 'color'}" readonly="1"/>
                <field name="state_id" widget="badge" readonly="1" optional="hide"/>
                <field name="prescription_renewal_due_soon" column_invisible="1"/>
                <field name="prescription_renewal_overdue" column_invisible="1"/>
                <field name="prescription_renewal_total" column_invisible="1"/>
                <field name="prescription_properties"/>
                <field name="prescription_state" widget="badge" decoration-info="prescription_state == 'open'" decoration-danger="prescription_state == 'expired'" optional="hide"/>
                <field name="activity_exception_decoration" widget="activity_exception"/>
            </tree>
        </field>
    </record>

    <record id="prescription_type_view_search" model="ir.ui.view">
        <field name="name">prescription.type.search</field>
        <field name="model">prescription.type</field>
        <field name="arch" type="xml">
            <search string="All Prescription">
                <field string="Prescription" name="name" filter_domain="['|', ('name', 'ilike', self), ('prescription_id', 'ilike', self)]"/>
                <field string="Practitioners" name="log_practitioners" filter_domain="[
                    '|', '|','|','|',
                    ('log_practitioners.location_id', 'ilike', self),
                    ('location_id', 'ilike', self),
                    ('practitioner_id', 'ilike', self),
                    ('practitioner_employee_id', 'ilike', self),
                    ('future_practitioner_employee_id', 'ilike', self),
                ]"/>
                <field string="Model" name="model_id"/>
                <field string="RX ID" name="prescription_id"/>
                <field name="tag_ids"/>
                <field string="Status" name="state_id"/>
                <field string="Properties" name="prescription_properties"/>
                <filter string="Available" name="available" domain="['&amp;', ('practitioner_id', '=', False), '|', ('location_id', '=', False), '|', '&amp;', ('plan_to_change_prescription', '=', True), ('prescription_type', '=', 'custom'), '&amp;', ('plan_to_change_otc', '=', True), ('prescription_type', '=', 'otc')]"/>
                <filter string="OTCs" name="otcs" domain="[('prescription_type', '=', 'otc')]"/>
                <filter string="Prescription" name="prescription" domain="[('prescription_type', '=', 'custom')]"/>
                <filter string="Trailer Hook" name="trailer_hook" domain="[('trailer_hook', '=', True)]"/>
                <filter name="planned" string="Planned for Change" domain="['|', '&amp;', ('prescription_type', '=', 'otc'), ('plan_to_change_otc', '=', True), '&amp;', ('prescription_type', '=', 'custom'), ('plan_to_change_prescription', '=', True)]"/>
                <separator/>
                <filter string="Need Action" name="alert_true" domain="['|', ('prescription_renewal_due_soon', '=', True), ('prescription_renewal_overdue', '=', True)]"/>
                <separator/>
                <filter string="Archived" name="inactive" domain="[('active', '=', False)]"/>
                <separator/>
                <filter invisible="1" string="Late Activities" name="activities_overdue" domain="[('my_activity_date_deadline', '&lt;', context_today().strftime('%Y-%m-%d'))]" help="Show all records which has next action date is before today"/>
                <filter invisible="1" string="Today Activities" name="activities_today" domain="[('my_activity_date_deadline', '=', context_today().strftime('%Y-%m-%d'))]"/>
                <filter invisible="1" string="Future Activities" name="activities_upcoming_all" domain="[('my_activity_date_deadline', '&gt;', context_today().strftime('%Y-%m-%d'))]"/>
                <group expand="1" string="Group By">
                    <filter string="Status" name="groupby_status" context="{'group_by': 'state_id'}"/>
                    <filter string="Model" name="groupby_model" context="{'group_by': 'model_id'}"/>
                    <filter string="Line" name="groupby_make" context="{'group_by': 'line_id'}"/>
                    <filter string="Shell Type" name="groupby_shelltype" context="{'group_by': 'shell_type'}"/>
                </group>
            </search>
        </field>
    </record>


    <record id='prescription_type_view_kanban' model='ir.ui.view'>
        <field name="name">prescription.type.kanban</field>
        <field name="model">prescription.type</field>
        <field name="arch" type="xml">
            <kanban default_group_by="state_id" sample="1">
                <field name="prescription_id" />
                <field name="model_id" />
                <field name="location_id" />
                <field name="practitioner_id" />
                <field name="location" />
                <field name="state_id" />
                <field name="id" />
                <field name="tag_ids" />
                <field name="prescription_renewal_due_soon" />
                <field name="prescription_renewal_overdue" />
                <field name="prescription_renewal_name" />
                <field name="prescription_renewal_total" />
                <field name="prescription_count" />
                <field name="activity_ids"/>
                <field name="activity_state"/>
                <progressbar field="activity_state" colors='{"planned": "success", "today": "warning", "overdue": "danger"}'/>

                <templates>
                    <t t-name="kanban-box">
                        <div class="oe_kanban_global_click o_kanban_record_has_image_fill">
                            <div class="o_kanban_image">
                                <img t-attf-src="#{kanban_image('prescription.type', 'image_128', record.id.raw_value)}" t-att-alt="record.id.value"/>
                            </div>
                            <div class="oe_kanban_details">
                                <strong class="o_kanban_record_title">
                                    <t t-if="record.prescription_id.raw_value">
                                        <field name="prescription_id"/>
:</t>
                                    <field name="model_id"/>
                                </strong>
                                <div class="o_kanban_tags_section">
                                    <field name="tag_ids" widget="many2many_tags" options="{'color_field': 'color'}"/>
                                </div>
                                <ul>
                                    <li>
                                        <t t-if="record.location_id.raw_value">
                                            <field name="location_id" widget="many2one_avatar" options="{'display_avatar_name': True}"/>
                                        </t>
                                    </li>
                                    <li>
                                        <t t-if="record.practitioner_id.raw_value">Practitioner : <field name="practitioner_id"/>
                                        </t>
                                    </li>
                                    <li>
                                        <t t-if="record.location.raw_value">
                                            <small>
                                                <i class="fa fa-map-marker" title="Location"></i>
                                                <field name="location"/>
                                            </small>
                                        </t>
                                    </li>
                                    <li>
                                        <field name="prescription_properties" widget="properties"/>
                                    </li>
                                </ul>
                                <div class="o_kanban_record_bottom" t-if="!selection_mode">
                                    <div class="oe_kanban_bottom_left">
                                        <a t-if="record.prescription_count.raw_value>0" data-type="object" data-name="return_action_to_open" href="#" class="oe_kanban_action oe_kanban_action_a" data-context='{"xml_id":"prescription_type_log_action"}'>
                                            <field name="prescription_count"/>
                                            Prescription(s)
                                            <span t-if="record.prescription_renewal_due_soon.raw_value and !record.prescription_renewal_overdue.raw_value" class="fa fa-exclamation-triangle text-warning" role="img" aria-label="Warning: renewal due soon" title="Warning: renewal due soon">
                                            </span>
                                            <span t-if="record.prescription_renewal_overdue.raw_value" class="fa fa-exclamation-triangle text-danger" role="img" aria-label="Attention: renewal overdue" title="Attention: renewal overdue">
                                            </span>
                                        </a>
                                        <field name="activity_ids" widget="kanban_activity"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <record id="prescription_type_view_activity" model="ir.ui.view">
        <field name="name">prescription.type.activity</field>
        <field name="model">prescription.type</field>
        <field name="arch" type="xml">
            <activity string="Prescription">
                <field name="prescription_id"/>
                <field name="id"/>
                <templates>
                    <div t-name="activity-box">
                        <img class="rounded-circle" t-att-src="activity_image('prescription.type', 'image_128', record.id.raw_value)" role="img" t-att-title="record.id.value" t-att-alt="record.id.value"/>
                        <div class="ms-2">
                            <field name="prescription_id" display="full" class="o_text_block"/>
                            <field name="model_id" muted="1" class="o_text_block"/>
                        </div>
                    </div>
                </templates>
            </activity>
        </field>
    </record>

    <record id="prescription_type_view_pivot" model="ir.ui.view">
        <field name="model">prescription.type</field>
        <field name="arch" type="xml">
            <pivot>
                <field name="state_id" type="col" />
                <field name="line_id" type="row" />
                <field name="model_id" type="row" />
                <field name="prescription_id" type="row" />
            </pivot>
        </field>
    </record>

    <record id='prescription_type_action' model='ir.actions.act_window'>
        <field name="name">Prescription</field>
        <field name="res_model">prescription.type</field>
        <field name="view_mode">kanban,tree,form,pivot,activity</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Ready to manage your prescription more efficiently?
            </p>
            <p>
            Let's create your first prescription.
            </p>
        </field>
    </record>

    <menuitem name="Prescription" parent="menu_root" id="prescription_type" sequence="2" groups="prescription_group_user"/>
    <menuitem action="prescription_type_action" parent="prescription_type" name="Prescription" id="prescription_type_menu" groups="prescription_group_user" sequence="0"/>

    <record id='prescription_type_measure_view_form' model='ir.ui.view'>
        <field name="name">prescription.type.measure.form</field>
        <field name="model">prescription.type.measure</field>
        <field name="arch" type="xml">
            <form string="Measurement Logs">
                <sheet>
                    <group>
                        <group>
                            <field name="prescription_id"/>
                            <label for="value"/>
                            <div class="o_row">
                                <field name="value" class="oe_inline"/>
                                <field name="unit" class="ms-2"/>
                            </div>
                            <field name="date"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id='prescription_type_measure_view_tree' model='ir.ui.view'>
        <field name="name">prescription.type.measure.tree</field>
        <field name="model">prescription.type.measure</field>
        <field name="arch" type="xml">
            <tree string="Measurement Logs" editable="top">
                <field name="date" />
                <field name="prescription_id" widget="many2one_avatar"/>
                <field name="location_id" widget="many2one_avatar"/>
                <field name="value" />
                <field name="unit" />
            </tree>
        </field>
    </record>

    <record id='prescription_type_measure_view_kanban' model='ir.ui.view'>
        <field name="name">prescription.type.measure.kanban</field>
        <field name="model">prescription.type.measure</field>
        <field name="arch" type="xml">
            <kanban class="o_kanban_mobile">
                <templates>
                    <t t-name="kanban-box">
                        <div t-attf-class="oe_kanban_global_click">
                            <div>
                                <strong>
                                    <field name="prescription_id"/>
                                    <span class="float-end">
                                        <field name="date"/>
                                    </span>
                                </strong>
                            </div>
                            <div>
                                <span>
                                    <field name="location_id"/>
                                </span>
                                <span class="float-end">
                                    <field name="value"/>
 Km</span>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <record id='prescription_type_measure_view_search' model='ir.ui.view'>
        <field name="name">prescription.type.measure.search</field>
        <field name="model">prescription.type.measure</field>
        <field name="arch" type="xml">
            <search string="Prescription Measures">
                <field name="prescription_id"/>
                <field name="location_id"/>
                <field name="value"/>
                <field name="date"/>
                <group expand="0" string="Group By">
                    <filter name="groupby_prescription" context="{'group_by': 'prescription_id'}" string="Prescription"/>
                </group>
            </search>
        </field>
    </record>

    <record id="prescription_type_measure_view_graph" model="ir.ui.view">
        <field name="name">prescription.type.measure.graph</field>
        <field name="model">prescription.type.measure</field>
        <field name="arch" type="xml">
            <graph string="Measurement Values Per Prescription" sample="1">
                <field name="prescription_id"/>
                <field name="value" type="measure"/>
            </graph>
        </field>
    </record>

    <record id='prescription_type_measure_action' model='ir.actions.act_window'>
        <field name="name">Measures</field>
        <field name="res_model">prescription.type.measure</field>
        <field name="view_mode">tree,kanban,form,graph</field>
        <field name="context"></field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Create a new measure log
            </p>
            <p>
            You can add various measure entries for all prescription.
            </p>
        </field>
    </record>

    <menuitem action="prescription_type_measure_action" parent="prescription_type" id="prescription_type_measure_menu" groups="prescription_group_user" sequence="10"/>

    <record id='prescription_type_service_types_view_tree' model='ir.ui.view'>
        <field name="name">prescription.service.type.tree</field>
        <field name="model">prescription.service.type</field>
        <field name="arch" type="xml">
            <tree string="Adjustment Types" editable="bottom">
                <field name="name" />
                <field name="category"/>
            </tree>
        </field>
    </record>

    <record id="prescription_type_service_types_view_search" model="ir.ui.view">
        <field name="model">prescription.service.type</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>
                <field name="category"/>
                <group expand="1" string="Group By">
                    <filter name="groupby_category" context="{'group_by' : 'category'}"/>
                </group>
            </search>
        </field>
    </record>

    <record id='prescription_type_service_types_action' model='ir.actions.act_window'>
        <field name="name">Types</field>
        <field name="res_model">prescription.service.type</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{"search_default_groupby_category" : True}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Create a new type of service
            </p>
            <p>
            Each service can used in prescription, as a standalone service or both.
            </p>
        </field>
    </record>
    <menuitem name="Adjustments" parent="prescription_configuration" id="prescription_services_configuration" sequence="20" groups="base.group_no_one"/>
    <menuitem action="prescription_type_service_types_action" parent="prescription_services_configuration" name="Types" id="prescription_type_service_types_menu" sequence="1" groups="base.group_no_one"/>

    <record id='prescription_type_state_view_tree' model='ir.ui.view'>
        <field name="name">prescription.type.state.tree</field>
        <field name="model">prescription.type.state</field>
        <field name="arch" type="xml">
            <tree string="State" editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id='prescription_type_state_action' model='ir.actions.act_window'>
        <field name="name">Status</field>
        <field name="res_model">prescription.type.state</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
            Create a new prescription status
            </p>
            <p>
