<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="0">

    <record id="product_template_attribute_line_laterality" model="product.template.attribute.line" context="{'create_product_product': False}">
        <field name="product_tmpl_id" ref="product_template_superglass_flex"/>
        <field name="attribute_id" ref="product_attribute_laterality"/>
        <field name="value_ids" eval="[(6, 0, [
        ref('product_attribute_value_lt'), 
        ref('product_attribute_value_rt'), 
        ref('product_attribute_value_bl'),
        ])]"/>
    </record>
    <record id="product_template_attribute_value_lt" model="product.template.attribute.value">
        <field name="attribute_line_id" ref="product_template_attribute_line_laterality"/>
        <field name="product_attribute_value_id" ref="product_attribute_value_lt"/>
        <field name="color">5</field>
    </record>
    <record id="product_template_attribute_value_rt" model="product.template.attribute.value">
        <field name="attribute_line_id" ref="product_template_attribute_line_laterality"/>
        <field name="product_attribute_value_id" ref="product_attribute_value_rt"/>
        <field name="color">5</field>
    </record>
    <record id="product_template_attribute_value_bl" model="product.template.attribute.value">
        <field name="attribute_line_id" ref="product_template_attribute_line_laterality"/>
        <field name="product_attribute_value_id" ref="product_attribute_value_bl"/>
        <field name="price_extra">90.00</field>
        <field name="color">5</field>
    </record>

    <record id="product_template_attribute_line_arch_height" model="product.template.attribute.line" context="{'create_product_product': False}">
        <field name="product_tmpl_id" ref="product_template_superglass_flex"/>
        <field name="attribute_id" ref="product_attribute_arch_height"/>
        <field name="value_ids" eval="[(6, 0, [
        ref('product_attribute_value_standard'), 
        ref('product_attribute_value_sulcus'), 
        ref('product_attribute_value_full'), 
        ref('product_attribute_value_complete'),
        ])]"/>
    </record>
    <record id="product_template_attribute_value_standard" model="product.template.attribute.value">
        <field name="attribute_line_id" ref="product_template_attribute_line_arch_height"/>
        <field name="product_attribute_value_id" ref="product_attribute_value_standard"/>
    </record>
    <record id="product_template_attribute_value_sulcus" model="product.template.attribute.value">
        <field name="attribute_line_id" ref="product_template_attribute_line_arch_height"/>
        <field name="product_attribute_value_id" ref="product_attribute_value_sulcus"/>
    </record>
    <record id="product_template_attribute_value_full" model="product.template.attribute.value">
        <field name="attribute_line_id" ref="product_template_attribute_line_arch_height"/>
        <field name="product_attribute_value_id" ref="product_attribute_value_full"/>
    </record>
    <record id="product_template_attribute_value_complete" model="product.template.attribute.value">
        <field name="attribute_line_id" ref="product_template_attribute_line_arch_height"/>
        <field name="product_attribute_value_id" ref="product_attribute_value_complete"/>
    </record>


</odoo>
