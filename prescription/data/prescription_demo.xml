<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="base.user_demo" model="res.users">
        <field name="groups_id" eval="[(3, ref('prescription.prescription_group_manager'))]"/>
    </record>

    <record id="prescription_type_state_ordered" model="prescription.type.state">
        <field name="name">Ordered</field>
        <field name="sequence">6</field>
    </record>

    <record id="prescription_type_state_reserve" model="prescription.type.state">
        <field name="name">Reserve</field>
        <field name="sequence">9</field>
    </record>

    <record id="prescription_type_state_on_hold" model="prescription.type.state">
        <field name="name">On Hold</field>
        <field name="sequence">10</field>
    </record>

    <record id="type_service_service_1" model="prescription.service.type">
        <field name="name">Repair</field>
        <field name="category">service</field>
    </record>

    <record id="model_category_1" model="prescription.type.model.category">
        <field name="name">Custom</field>
        <field name="color" eval="1"/>
    </record>

    <record id="model_category_2" model="prescription.type.model.category">
        <field name="name">OTC</field>
        <field name="color" eval="2"/>
    </record>

    <record id="model_category_3" model="prescription.type.model.category">
        <field name="name">Brace</field>
        <field name="color" eval="3"/>
    </record>

    <record id="model_category_4" model="prescription.type.model.category">
        <field name="name">Blanks</field>
        <field name="color" eval="4"/>
    </record>

    <record id="prescription_type_tag_sg" model="prescription.type.tag">
        <field name="name">Superglass</field>
        <field name="color" eval="1"/>
    </record>

    <record id="prescription_type_tag_poly" model="prescription.type.tag">
        <field name="name">Polypropylene</field>
        <field name="color" eval="2"/>
    </record>

    <record id="prescription_type_tag_ncv" model="prescription.type.tag">
        <field name="name">NCV</field>
        <field name="color" eval="3"/>
    </record>

    <record id="prescription_type_tag_pc" model="prescription.type.tag">
        <field name="name">Prescription Comfort</field>
        <field name="color" eval="4"/>
    </record>

    <record id="prescription_1" model="prescription.type">
        <field name="prescription_id">1-SGF-001</field>
        <field name="oin_sn">455257985</field>
        <field name="model_id" ref="model_superglass_flex"/>
        <field name="color">White</field>
        <field name="location">Grand-Rosiere</field>
        <field name="qty">1</field>
        <field name="location_id" ref="base.res_partner_address_16" />
        <field name="order_date" eval="time.strftime('%Y-%m-%d 2:00:00')" />
        <field name="received_date" eval="time.strftime('%Y-%m-%d 2:00:00')" />
        <field name="state_id" ref="prescription_type_state_draft"/>
        <field name="laterality">lt</field>
        <field name="prescription_price">90</field>
        <field eval="[(6,0,[ref('prescription.prescription_type_tag_sg')])]" name="tag_ids"/>
    </record>

</odoo>
