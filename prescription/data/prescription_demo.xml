        <!-- Prescription -->
        <record id="prescription.prescription_0" model="prescription.prescription">
            <field name="name">Design Fair Los Angeles</field>
            <field name="user_id" ref="base.user_demo"/>
            <field name="date_begin" eval="(DateTime.now() + timedelta(days=10)).strftime('%Y-%m-%d 08:00:00')"/>
            <field name="date_end" eval="(DateTime.now() + timedelta(days=14)).strftime('%Y-%m-%d 18:00:00')"/>
            <field name="seats_limited">True</field>
            <field name="seats_max">50</field>
            <field name="address_id" ref="prescription.res_partner_location_2"/>
            <field name="date_tz">US/Pacific</field>
            <field name="prescription_type_id" ref="prescription_type_0"/>
            <field name="stage_id" ref="prescription_stage_booked"/>
            <field name="tag_ids" eval="[(4, ref('prescription.prescription_tag_category_1_tag_1')), (4, ref('prescription.prescription_tag_category_2_tag_1'))]"/>
            <field name="device_instructions" type="html">
                <div class="text-center font-weight-bold py-3">Important device information</div>
                <ul>
                    <li>Please come <b>at least</b> 30 minutes before the beginning of the prescription.</li>
                    <li>Devices can be printed or scanned directly from your phone.</li>
                    <li>If you don't have this device, you will <b>not</b> be allowed entry!</li>
                </ul>
            </field>
        </record>
