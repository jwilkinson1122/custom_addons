<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Prescription Mail Scheduler-->
        <record model="ir.cron" forcecreate="True" id="prescription_mail_scheduler">
            <field name="name">Prescription: Mail Scheduler</field>
            <field name="model_id" ref="model_prescription_mail"/>
            <field name="state">code</field>
            <field name="code">model.schedule_communications(autocommit=True)</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
        </record>
    </data>
</odoo>
