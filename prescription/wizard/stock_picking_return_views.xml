<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_stock_return_picking_form" model="ir.ui.view">
        <field name="name">Return lines inherit Prescription</field>
        <field name="model">stock.return.picking</field>
        <field name="inherit_id" ref="stock.view_stock_return_picking_form" />
        <field name="arch" type="xml">
            <field name="product_return_moves" position="after">
                <group name="group_prescription">
                    <field name="create_prescription" invisible="picking_type_code != 'outgoing'"/>
                    <field name="prescription_location_ids" invisible="1" />
                    <field name="picking_id" invisible="1" />
                    <field name="picking_type_code" invisible="1" />
                </group>
            </field>
        </field>
    </record>
</odoo>
