<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <!-- Multicompay rules-->
    <record model="ir.rule" id="prescription_order_rule">
        <field name="name">prescription order multi-company</field>
        <field name="model_id" search="[('model','=','prescription.order')]" model="ir.model"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

</odoo>
