<odoo noupdate="1">

  <record id="stage_new" model="clinic.bookout.stage">
    <field name="name">New</field>
    <field name="sequence">10</field>
    <field name="state">new</field>
  </record>

  <record id="stage_open" model="clinic.bookout.stage">
    <field name="name">Received</field>
    <field name="sequence">20</field>
    <field name="state">open</field>
  </record>
  <record id="stage_done" model="clinic.bookout.stage">
    <field name="name">Complete</field>
    <field name="sequence">90</field>
    <field name="state">done</field>
  </record>
  <record id="stage_cancel" model="clinic.bookout.stage">
    <field name="name">Cancelled</field>
    <field name="sequence">95</field>
    <field name="state">cancel</field>
  </record>

</odoo>
