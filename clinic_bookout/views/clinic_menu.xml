<odoo>
    <record id="action_clinic_bookout" model="ir.actions.act_window">
        <field name="name">Bookouts</field>
        <field name="res_model">clinic.bookout</field>
        <field name="view_mode">kanban,tree,form,calendar,pivot,graph,activity</field>
    </record>

    <menuitem id="menu_clinic_bookout" name="Bookout" action="action_clinic_bookout" parent="clinic_app.menu_clinic" sequence="3"/>

    <record id="action_clinic_stage" model="ir.actions.act_window">
        <field name="name">Stages</field>
        <field name="res_model">clinic.bookout.stage</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_clinic_stage" name="Stages" action="action_clinic_stage" parent="clinic_app.menu_clinic_configuration" />
</odoo>
