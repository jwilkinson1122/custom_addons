<odoo noupdate="1">

  <record id="stage_new" model="pod.checkout.stage">
    <field name="name">New</field>
    <field name="sequence">10</field>
    <field name="state">new</field>
  </record>

  <record id="stage_open" model="pod.checkout.stage">
    <field name="name">Open</field>
    <field name="sequence">20</field>
    <field name="state">open</field>
  </record>
  <record id="stage_done" model="pod.checkout.stage">
    <field name="name">Done</field>
    <field name="sequence">90</field>
    <field name="state">done</field>
  </record>
  <record id="stage_cancel" model="pod.checkout.stage">
    <field name="name">Canceled</field>
    <field name="sequence">95</field>
    <field name="state">cancel</field>
  </record>

</odoo>
