<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="invoice_form_inherited" model="ir.ui.view">
            <field name="name">account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='invoice_payment_term_id']" position="after">
                    <field name="lice_number"/>
                </xpath>

                <xpath expr="/form/sheet/notebook/page/field[@name='invoice_line_ids']/tree/field[@name='name']"
                       position="after">
                    <field name="batch_number"/>
                    <field name="mfg_date"/>
                    <field name="exp_date"/>
                    <field name="pack_size"/>
                    <field name="mrp"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>