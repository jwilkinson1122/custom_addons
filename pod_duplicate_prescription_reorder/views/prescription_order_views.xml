<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <record id="inherit_prescription_form_view" model="ir.ui.view">
            <field name="name">inherit.prescription.order.form</field>
            <field name="model">prescription.order</field>
            <field name="inherit_id" ref="pod_prescription.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='pricelist_id']" position="after">
                    <field name="is_reorder" invisible="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>


