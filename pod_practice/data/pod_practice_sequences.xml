<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        <!-- Sequences for practice test report -->
        <record id="seq_pod_practice_test" model="ir.sequence">
            <field name="name">Practice Test</field>
            <field name="code">pod.practice.test.result</field>
            <field name="prefix">TEST</field>
            <field name="padding">3</field>
        </record>
        <!-- Sequences for practice request -->
        <record id="seq_pod_practice_request" model="ir.sequence">
            <field name="name">Rx Request</field>
            <field name="code">pod.practice.test.requests</field>
            <field name="prefix">RxRQ</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>
