<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="podiatry_patient_form" model="ir.ui.view">
        <field name="name">podiatry.patient.form</field>
        <field name="model">podiatry.patient</field>
        <field name="inherit_id" ref="podiatry_administration.podiatry_patient_form" />
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="%(podiatry_workflow.action_podiatry_add_plan_definition_form)d" type="action" string="Add Plan definition" groups="podiatry_workflow.group_patient_add_plan_definition" class="oe_highlight" />
            </xpath>
        </field>
    </record>
</odoo>
