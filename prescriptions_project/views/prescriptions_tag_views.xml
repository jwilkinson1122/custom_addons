<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tag_view_form_inherit" model="ir.ui.view">
        <field name="name">tag.view.form.inherit</field>
        <field name="model">prescriptions.tag</field>
        <field name="inherit_id" ref="prescriptions.tag_view_form"/>
        <field name="mode">primary</field>
        <field name="priority">999</field>
        <field name="arch" type="xml">
            <field name="facet_id" position="attributes">
                <attribute name="context">{'form_view_ref': 'prescriptions_project.facet_view_form_with_folder_inherit', 'prescriptions_project_folder': context.get('prescriptions_project_folder')}</attribute>
            </field>
        </field>
    </record>
</odoo>
