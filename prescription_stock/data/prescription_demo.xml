        <!-- Change date of those prescription orders' delivery -->
        <function model="stock.picking" name="write">
            <value model="stock.picking" search="[('prescription_id', '=', obj().env.ref('prescription_stock.prescription_19').id)]"/>
            <value eval="{'scheduled_date': (datetime.now() + relativedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')}"/>
        </function>
        <function model="stock.picking" name="write">
            <value model="stock.picking" search="[('prescription_id', '=', obj().env.ref('prescription_stock.prescription_20').id)]"/>
