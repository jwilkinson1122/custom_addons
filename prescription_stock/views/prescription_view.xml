<odoo>
    <data>
        <record id="prescription_stock.type_form" model="ir.ui.view">
            <field name="name">prescription_stock.prescription_type_form</field>
            <field name="inherit_id" ref="prescription.prescription_type_view_form" />
            <field name="model">prescription.type</field>
            <field eval="20" name="priority"/>
            <!-- <field name="type">form</field> -->
            <field name="arch" type="xml">
                <button name="open_assignation_logs" position="after">
                    <button name="act_show_pickings" type="object" class="oe_stat_button" icon="fa-truck">
                        <field name="picking_count" widget="statinfo" string="Pickings"/>
                    </button>
                </button>
                <field name="practitioner_id" position="after">
                    <field name="picking_ok" />
                </field>
            </field>
        </record>
    </data>
</odoo>