<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="pod_clinical_finding_view_tree" model="ir.ui.view">
        <field name="name">pod.clinical.finding.view.tree</field>
        <field name="model">pod.clinical.finding</field>
        <field
            name="inherit_id"
            ref="pod_clinical_condition.pod_clinical_finding_view_tree"
        />
        <field name="arch" type="xml">
            <field name="create_warning" position="after">
                <field name="create_condition_from_clinical_impression" />
            </field>
        </field>
    </record>
    <record id="pod_clinical_finding_view_form" model="ir.ui.view">
        <field name="name">pod.clinical.finding.view.form</field>
        <field name="model">pod.clinical.finding</field>
        <field
            name="inherit_id"
            ref="pod_clinical_condition.pod_clinical_finding_view_form"
        />
        <field name="arch" type="xml">
            <field name="create_warning" position="after">
                <field name="create_condition_from_clinical_impression" />
            </field>
        </field>
    </record>
</odoo>
