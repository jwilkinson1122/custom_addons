<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record model="ir.ui.view" id="product_pod_center_company_form_view">
        <field name="name">product.pod.center.company.form (in nw_pod_pos)</field>
        <field name="model">product.pod.center.company</field>
        <field name="arch" type="xml">
            <form>
                <header />
                <sheet>
                    <group>
                        <field name="center_id" />
                        <field name="company_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="product_pod_center_company_tree_view">
        <field name="name">product.pod.center.company.tree (in nw_pod_pos)</field>
        <field name="model">product.pod.center.company</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="center_id" />
                <field name="company_id" />
            </tree>
        </field>
    </record>
    
</odoo>
