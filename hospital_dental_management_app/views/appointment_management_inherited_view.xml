<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record model="ir.ui.view" id="inherited_appointment_management_view_form">
            <field name="name">appointment.management.inherited.form</field>
            <field name="model">appointment.management</field>
            <field name="inherit_id" ref="hospital_management_app.appointment_management_view_form_new"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="attributes">
                    <attribute name="string">Dentist</attribute>
                </field>
                <field name="disease_type_id" position="attributes">
                    <attribute name="string">Teeth Problem</attribute>
                </field>
                <field name="disease_stage_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <field name="disease_fees" position="attributes">
                    <attribute name="string">Fees Per Visit</attribute>
                </field>
            </field>
        </record>
        <record model="ir.ui.view" id="inherited_appointment_management_view_tree">
            <field name="name">appointment.management.inherited.tree</field>
            <field name="model">appointment.management</field>
            <field name="inherit_id" ref="hospital_management_app.appointment_management_view_tree_new"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="attributes">
                    <attribute name="string">Dentist</attribute>
                </field>
                <field name="disease_type_id" position="attributes">
                    <attribute name="string">Teeth Problem</attribute>
                </field>
                <field name="disease_stage_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
            </field>
        </record>
        <record model="ir.ui.view" id="inherited_appointment_management_view_search">
            <field name="name">appointment.management.inherited.search</field>
            <field name="model">appointment.management</field>
            <field name="inherit_id" ref="hospital_management_app.appointment_management_view_search_new"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="attributes">
                    <attribute name="string">Dentist</attribute>
                </field>
                <field name="disease_type_id" position="attributes">
                    <attribute name="string">Teeth Problem</attribute>
                </field>
                <field name="disease_stage_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <xpath expr="//search/group/filter[@name='partner_id']" position="attributes">
                    <attribute name="string">Dentist</attribute>
                </xpath>
                <xpath expr="//search/group/filter[@name='disease_type_id']" position="attributes">
                    <attribute name="string">Teeth Problem</attribute>
                </xpath>
                <xpath expr="//search/group/filter[@name='disease_stage_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>


        <record model="ir.actions.act_window" id="hospital_management_app.action_disease_type">
            <field name="name">Teeth Problem</field>
        </record>
        <record model="ir.actions.act_window" id="hospital_management_app.action_contact_doctor">
            <field name="name">Dentist</field>
        </record>
        <record model="ir.ui.menu" id="hospital_management_app.menu_disease_type">
            <field name="name">Teeth Problem</field>
        </record>
        <record model="ir.ui.menu" id="hospital_management_app.menu_disease_stage">
            <field name="action" eval="False"/>
        </record>
        <record model="ir.ui.menu" id="hospital_management_app.menu_contact_doctor">
            <field name="name">Dentist</field>
        </record>
        <record model="ir.ui.menu" id="hospital_management_app.main_menu_hospital_management">
            <field name="name">Hospital Dentist</field>
        </record>


    </data>
</odoo>
