<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record model="ir.ui.view" id="inherited_patient_procedures_view_form">
            <field name="name">patient.procedures.inherited.form</field>
            <field name="model">patient.procedures</field>
            <field name="inherit_id" ref="hospital_management_app.patient_procedures_view_form_new"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="attributes">
                    <attribute name="string">Dentist</attribute>
                </field>
                <field name="condition_type_id" position="attributes">
                    <attribute name="string">Teeth Problem</attribute>
                </field>
                <field name="condition_stage_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
            </field>
        </record>
        <record model="ir.ui.view" id="inherited_patient_procedures_view_tree">
            <field name="name">patient.procedures.inherited.tree</field>
            <field name="model">patient.procedures</field>
            <field name="inherit_id" ref="hospital_management_app.patient_procedures_view_tree_new"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="attributes">
                    <attribute name="string">Dentist</attribute>
                </field>
                <field name="condition_type_id" position="attributes">
                    <attribute name="string">Teeth Problem</attribute>
                </field>
                <field name="condition_stage_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
            </field>
        </record>
        <record model="ir.ui.view" id="inherited_patient_procedures_view_search">
            <field name="name">patient.procedures.inherited.tree</field>
            <field name="model">patient.procedures</field>
            <field name="inherit_id" ref="hospital_management_app.patient_procedures_view_search_new"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="attributes">
                    <attribute name="string">Dentist</attribute>
                </field>
                <field name="condition_type_id" position="attributes">
                    <attribute name="string">Teeth Problem</attribute>
                </field>
                <field name="condition_stage_id" position="attributes">
                    <attribute name="invisible">1</attribute>
                </field>
                <xpath expr="//search/group/filter[@name='partner_id']" position="attributes">
                    <attribute name="string">Dentist</attribute>
                </xpath>
                <xpath expr="//search/group/filter[@name='condition_type_id']" position="attributes">
                    <attribute name="string">Teeth Problem</attribute>
                </xpath>
                <xpath expr="//search/group/filter[@name='condition_stage_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>


    </data>
</odoo>
