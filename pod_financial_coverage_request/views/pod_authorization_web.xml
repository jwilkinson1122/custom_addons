<odoo>
    <record model="ir.ui.view" id="pod_authorization_web_form_view">
        <field
            name="name"
        >pod.authorization.web.form (in pod_financial_coverage_request)</field>
        <field name="model">pod.authorization.web</field>
        <field name="arch" type="xml">
            <form>
                <header />
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="code" />
                        <field name="link" />
                        <field name="notes" />
                    </group>
                </sheet>
                <div class="oe_chatter" />
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="pod_authorization_web_search_view">
        <field
            name="name"
        >pod.authorization.web.search (in pod_financial_coverage_request)</field>
        <field name="model">pod.authorization.web</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
                <field name="code" />
                <field name="link" />
                <field name="notes" />
            </search>
        </field>
    </record>
    <record model="ir.ui.view" id="pod_authorization_web_tree_view">
        <field
            name="name"
        >pod.authorization.web.tree (in pod_financial_coverage_request)</field>
        <field name="model">pod.authorization.web</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="code" />
                <field name="link" />
                <field name="notes" />
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="pod_authorization_web_act_window">
        <field name="name">Podiatry Authorization Web</field>
        <field name="res_model">pod.authorization.web</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>
    <record model="ir.ui.menu" id="pod_authorization_web_menu">
        <field name="name">Podiatry Authorization Web</field>
        <field name="parent_id" ref="pod_base.pod_root_configuration" />
        <field name="action" ref="pod_authorization_web_act_window" />
        <field name="sequence" eval="16" />
    </record>
</odoo>
