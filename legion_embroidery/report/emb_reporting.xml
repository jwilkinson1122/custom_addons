<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_order_embroidery_pivot" model="ir.ui.view">
        <field name="name">embroidery.embroidery.pivot</field>
        <field name="model">embroidery.embroidery</field>
        <field name="arch" type="xml">
            <pivot string="Embroidery Analysis" disable_linking="True">
                <!--                <field name="partner_id" type="col"/>-->
                <!--                <field name="date_planned" interval="month" type="row"/>-->
                <field name="state" type="row"/>
                <field name="product_qty" type="measure"/>
            </pivot>
        </field>
    </record>

    <record id="view_order_embroidery_graph" model="ir.ui.view">
        <field name="name">embroidery.embroidery.graph</field>
        <field name="model">embroidery.embroidery</field>
        <field name="arch" type="xml">
            <graph string="Embroidery Analysis" type="line">
                <field name="date_planned" type="row" interval="day"/>
                <field name="product_qty" type="measure"/>
            </graph>
        </field>
    </record>

