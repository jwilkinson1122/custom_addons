<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="seq_pod_laboratory_request" model="ir.sequence">
        <field name="name">Podiatry Laboratory request sequence</field>
        <field name="code">pod.laboratory.request</field>
        <field name="prefix">LR</field>
        <field name="padding">8</field>
    </record>
    <record id="seq_pod_laboratory_event" model="ir.sequence">
        <field name="name">Podiatry Laboratory event sequence</field>
        <field name="code">pod.laboratory.event</field>
        <field name="prefix">LE</field>
        <field name="padding">8</field>
    </record>
</odoo>
