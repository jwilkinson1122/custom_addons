<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="patient_registration_sequence" model="ir.sequence">
            <field name="name">Patient Seq</field>
            <field name="code">medical.patient</field>
            <field name="prefix">PAC</field>
            <field name="padding">2</field>
        </record>

        
        <!--sequence for practice-->
        <record id="sequence_medical_practice" model="ir.sequence">
            <field name="name">Practice ID</field>
            <field name="code">medical.practice</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
        </record>

        <!--sequence for practitioner-->
        <record id="sequence_medical_physician" model="ir.sequence">
            <field name="name">Practitioner ID</field>
            <field name="code">medical.physician</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
        </record>

        

    </data>
</odoo>
