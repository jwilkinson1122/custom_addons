<odoo>
    <record model="ir.ui.view" id="pod_agreement_expand_form_view">
        <field
            name="name"
        >pod.agreement.expand.form (in pod_financial_coverage_agreement)</field>
        <field name="model">pod.agreement.expand</field>
        <field name="arch" type="xml">
            <form string="Podiatry Agreement Expand">
                <group>
                    <field name="agreement_id" invisible="1" />
                    <field name="name" />
                    <field name="date_to" />
                    <field name="difference" />
                </group>
                <footer>
                    <button
                        name="expand"
                        string="OK"
                        class="btn-primary"
                        type="object"
                    />
                    <button string="Cancel" class="btn-default" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
    <record model="ir.actions.act_window" id="pod_agreement_expand_act_window">
        <field name="name">Podiatry Agreement Expand</field>
        <field name="res_model">pod.agreement.expand</field>
        <field name="view_mode">form</field>
        <field name="context">{}</field>
        <field name="target">new</field>
    </record>
</odoo>
