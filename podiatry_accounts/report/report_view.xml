<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html>

<odoo>

        <record id="paperformat_margin" model="report.paperformat">
            <field name="name">Report margin</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">50</field>
            <field name="margin_bottom">30</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">45</field>
            <field name="dpi">80</field>
        </record>

        <record id="report_patient_payslip_qweb" model="ir.actions.report">
            <field name="name">Patient Payslip</field>
            <field name="model">patient.payslip</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">podiatry_accounts.patient_payslip</field>
            <field name="report_file">podiatry_accounts.patient_payslip</field>
            <field name="binding_model_id" ref="podiatry_accounts.model_patient_payslip"/>
            <field name="paperformat_id" ref="podiatry_accounts.paperformat_margin"/>
            <field name="binding_type">report</field>
        </record>

        <record id="report_patient_accounts_register_qweb" model="ir.actions.report">
            <field name="name">Accounts Register</field>
            <field name="model">patient.accounts.register</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">podiatry_accounts.patient_accounts_register</field>
            <field name="report_file">podiatry_accounts.patient_accounts_register</field>
            <field name="binding_model_id" ref="podiatry_accounts.model_patient_accounts_register"/>
            <field name="paperformat_id" ref="podiatry_accounts.paperformat_margin"/>
            <field name="binding_type">report</field>
        </record>

</odoo>
