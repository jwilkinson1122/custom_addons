<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html>

<odoo>
    <data noupdate="1">
        <record id="demo_product_accounts_1" model="product.product">
            <field name="name">Podiatry Accounts</field>
            <field name="code">FEEEDU</field>
            <field name="detailed_type">service</field>
            <field name="list_price">4000.00</field>
            <field name="categ_id" ref="podiatry_accounts.product_categ_accounts"/>
        </record>
        <record id="demo_product_accounts_2" model="product.product">
            <field name="name">Library Accounts</field>
            <field name="code">FEELIB</field>
            <field name="detailed_type">service</field>
            <field name="list_price">2000.00</field>
            <field name="categ_id" ref="podiatry_accounts.product_categ_accounts"/>
        </record>
        <record id="demo_product_accounts_3" model="product.product">
            <field name="name">Laboratory Accounts</field>
            <field name="code">FEELAB</field>
            <field name="detailed_type">service</field>
            <field name="list_price">3000.00</field>
            <field name="categ_id" ref="podiatry_accounts.product_categ_accounts"/>
        </record>

        <!--Patient Accounts Structure Line-->
        <record id="demo_patient_accounts_structure_line_1" model="patient.accounts.structure.line">
            <field name="name">Podiatry Accounts</field>
            <field name="code">01</field>
            <field name="type">month</field>
            <field name="product_id" ref="podiatry_accounts.demo_product_accounts_1"/>
            <field name="amount">4000.00</field>
        </record>
        <record id="demo_patient_accounts_structure_line_2" model="patient.accounts.structure.line">
            <field name="name">Library Accounts</field>
            <field name="code">02</field>
            <field name="type">month</field>
            <field name="product_id" ref="podiatry_accounts.demo_product_accounts_2"/>
            <field name="amount">2000.00</field>
        </record>
        <record id="demo_patient_accounts_structure_line_3" model="patient.accounts.structure.line">
            <field name="name">Laboratory Accounts</field>
            <field name="code">03</field>
            <field name="type">month</field>
            <field name="product_id" ref="podiatry_accounts.demo_product_accounts_3"/>
            <field name="amount">3000.00</field>
        </record>
        <!-- Accounts Structure Information -->
        <record id="demo_patient_accounts_structure_1" model="patient.accounts.structure">
            <field name="name">Patient Accounts Structure</field>
            <field name="code">SFS</field>
            <field eval="[(6,0,[ref('demo_patient_accounts_structure_line_1'), ref('demo_patient_accounts_structure_line_2'), ref('demo_patient_accounts_structure_line_3')])]" name="line_ids" />
        </record>
        <record id="demo_patient_accounts_structure_2" model="patient.accounts.structure">
            <field name="name">External Patient Accounts Structure</field>
            <field name="code">ESFS</field>
            <field eval="[(6,0,[ref('demo_patient_accounts_structure_line_1'), ref('demo_patient_accounts_structure_line_2')])]" name="line_ids"/>
        </record>
        <!--Patient accounts Registration -->
         <record id="demo_patient_accounts_registration_1" model="patient.accounts.register">
            <field name="name">Exam Accounts</field>
            <field name="date" eval="(DateTime.today())"/>
            <field name="accounts_structure" ref="podiatry_accounts.demo_patient_accounts_structure_2"/>
            <field name="standard_id" ref="podiatry.demo_standard_standard_2"/>
         </record>
         <record id="demo_patient_accounts_registration_2" model="patient.accounts.register">
            <field name="name">Exam Accounts</field>
            <field name="date" eval="(DateTime.today())"/>
            <field name="accounts_structure" ref="podiatry_accounts.demo_patient_accounts_structure_2"/>
            <field name="standard_id" ref="podiatry.demo_standard_standard_1"/>
         </record>

         <function model="patient.accounts.register" name="accounts_register_confirm"
         context="{'is_demo_record': 'true'}" eval="[[ref('demo_patient_accounts_registration_1'), ref('demo_patient_accounts_registration_2')]]"/>
    </data>
</odoo>
