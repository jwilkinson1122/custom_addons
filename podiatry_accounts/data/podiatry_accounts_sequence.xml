<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html>

<odoo>
    <data noupdate="1">

        <!-- Sequences For patient.payslip -->

        <record id="seq_patient_payslip" model="ir.sequence">
            <field name="name">Patient PaySlip</field>
            <field name="code">patient.payslip</field>
            <field name="prefix">SLIP/</field>
            <field name="padding">3</field>
        </record>

         <!-- Sequences For patient.accounts.register -->

        <record id="seq_patient_accounts_register" model="ir.sequence">
            <field name="name">Patient Accounts Register</field>
            <field name="code">patient.accounts.register</field>
            <field name="prefix">REG/</field>
            <field name="padding">3</field>
        </record>

    </data>
</odoo>
