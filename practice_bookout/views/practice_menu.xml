<odoo>
    <record id="action_practice_bookout" model="ir.actions.act_window">
        <field name="name">Bookouts</field>
        <field name="res_model">practice.bookout</field>
        <field name="view_mode">kanban,tree,form,calendar,pivot,graph,activity</field>
    </record>

    <menuitem id="menu_practice_bookout" name="Bookout" action="action_practice_bookout" parent="practice_app.menu_practice" />

