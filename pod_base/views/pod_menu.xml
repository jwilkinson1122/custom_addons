    <record model="ir.ui.menu" id="contacts.res_partner_menu_config">
        <field name="name">Configuration</field>
        <field name="parent_id" ref="contacts.menu_contacts" />
        <field name="groups_id" eval="[(6, 0, [ref('base.group_system')])]" />
        <field name="sequence" eval="2" />
    </record>

    <record model="ir.ui.menu" id="pod_root_clinical">
        <field name="name">Clinical</field>
        <field name="parent_id" ref="contacts.menu_contacts" />
        <field name="sequence" eval="10" />
        <field name="groups_id" eval="[
            (6, 0, [ref('group_pod_technician'),
                    ref('group_pod_practitioner'),
                    ref('group_pod_nurse'),
                    ref('group_pod_auxiliary'),
                    ref('group_pod_pharmacy')])]" />
    </record>

    <record model="ir.ui.menu" id="pod_clinical_request">
        <field name="name">Requests</field>
        <field name="parent_id" ref="pod_root_clinical" />
        <field name="sequence" eval="10" />
    </record>

    <record model="ir.ui.menu" id="pod_clinical_event">
        <field name="name">Events</field>
        <field name="parent_id" ref="pod_root_clinical" />
        <field name="sequence" eval="10" />
    </record>

    <record model="ir.ui.menu" id="pod_root_diagnostics">
        <field name="name">Diagnostics</field>
        <field name="parent_id" ref="contacts.menu_contacts" />
        <field name="sequence" eval="20" />
        <field name="groups_id" eval="[(6, 0, [ref('group_pod_practitioner'), ref('group_pod_technician')])]" />
    </record>

    <record model="ir.ui.menu" id="pod_root_devices">
        <field name="name">Devices</field>
        <field name="parent_id" ref="contacts.menu_contacts" />
        <field name="sequence" eval="30" />
        <field name="groups_id" eval="[(6, 0, [ref('group_pod_practitioner'), ref('group_pod_pharmacy')])]" />
    </record>

    <record model="ir.ui.menu" id="pod_root_financial">
        <field name="name">Financial</field>
        <field name="parent_id" ref="contacts.menu_contacts" />
        <field name="groups_id" eval="[(6, 0, [ref('group_pod_financial')])]" />
        <field name="sequence" eval="40" />
    </record>

    <record model="ir.ui.menu" id="pod_financial_coverage">
        <field name="name">Coverages</field>
        <field name="parent_id" ref="pod_base.pod_root_financial" />
        <field name="sequence" eval="10" />
    </record>

    <record model="ir.ui.menu" id="pod_root_workflow">
        <field name="name">Workflow</field>
        <field name="parent_id" ref="contacts.menu_contacts" />
        <field name="groups_id" eval="[(6, 0, [ref('group_pod_financial')])]" />
        <field name="sequence" eval="50" />
    </record>

    <record model="ir.ui.menu" id="pod_root_terminologies">
        <field name="name">Terminologies</field>
        <field name="parent_id" ref="contacts.menu_contacts" />
        <field name="groups_id" eval="[(6, 0, [ref('group_pod_configurator')])]" />
        <field name="sequence" eval="60" />
    </record>

    <menuitem id="pod_role_menu" name="Roles" parent="contacts.res_partner_menu_config" action="pod_base.pod_role_action" sequence="0" />
