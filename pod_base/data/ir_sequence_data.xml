<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="seq_pod_practice" model="ir.sequence">
        <field name="name">Practice sequence</field>
        <field name="code">pod.practice</field>
        <field name="prefix">MP</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_pod_location" model="ir.sequence">
        <field name="name">Location sequence</field>
        <field name="code">pod.location</field>
        <field name="prefix">ML</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_pod_practitioner" model="ir.sequence">
        <field name="name">Practitioner sequence</field>
        <field name="code">pod.practitioner</field>
        <field name="prefix">PT</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_pod_patient" model="ir.sequence">
        <field name="name">Patient sequence</field>
        <field name="code">pod.patient</field>
        <field name="prefix">PID</field>
        <field name="padding">6</field>
    </record>
</odoo>
