<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.menu" id="contacts.menu_contacts">
        <field name="name">Contacts</field>
    </record>

    <record model="ir.ui.menu" id="pod_root_menu_administration">
        <field name="name">Administration</field>
        <field name="parent_id" ref="contacts.menu_contacts" />
        <field name="groups_id" eval="[(6, 0, [ref('group_pod_configurator')])]" />
        <field name="sequence" eval="1" />
    </record>

    <record id="pod_root_menu_administration" model="ir.ui.menu">
        <field name="active" eval="True" />
    </record>

    <menuitem id="contacts.res_partner_menu_contacts" name="All Contacts" action="contacts.action_contacts" parent="pod_root_menu_administration" sequence="1"/>
    <menuitem id="pod_practice_menu" name="Practices" action="pod_prescriptions_contact.pod_practice_action" parent="pod_root_menu_administration" sequence="2"/>
    <menuitem id="pod_location_menu" name="Locations" action="pod_prescriptions_contact.pod_location_action" parent="pod_root_menu_administration" sequence="3"/>
    <menuitem id="pod_practitioner_menu" name="Contacts" action="pod_prescriptions_contact.pod_practitioner_action" parent="pod_root_menu_administration" sequence="4"/>
    <menuitem id="pod_role_menu" name="Roles" parent="contacts.res_partner_menu_config" action="pod_prescriptions_contact.pod_role_action" sequence="0" />
    <menuitem id="pod_diagnosis_menu" name="Diagnosis" parent="contacts.res_partner_menu_config" action="pod_prescriptions_contact.pod_diagnosis_action" sequence="1" />


