<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="frame_brand_view_form" model="ir.ui.view">
            <field name="name">frame.brand.view.form</field>
            <field name="model">frame.brand</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="image" widget="image"/>
                        </group>
                    </group>
                </form>
            </field>
        </record>

        <record id="frame_collection_view_form" model="ir.ui.view">
            <field name="name">frame.collection.view.form</field>
            <field name="model">frame.collection</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="brand"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="frame_brand_action" model="ir.actions.act_window">
            <field name="name">brand</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">frame.brand</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="frame_collection_action" model="ir.actions.act_window">
            <field name="name">collection</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">frame.collection</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="frame_material_action" model="ir.actions.act_window">
            <field name="name">frame_material</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">frame.material</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="frame_shape_action" model="ir.actions.act_window">
            <field name="name">frame_shape</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">frame.shape</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="frame_type_action" model="ir.actions.act_window">
            <field name="name">frame_type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">frame.type</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="frame_manufacturer_action" model="ir.actions.act_window">
            <field name="name">manufacturer</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">frame.manufacturer</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="lens_type_action" model="ir.actions.act_window">
            <field name="name">Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">lens.type</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="lens_style_action" model="ir.actions.act_window">
            <field name="name">Style</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">lens.style</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="lens_material_action" model="ir.actions.act_window">
            <field name="name">Material</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">lens.material</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="contact_lens_manufacturer_action" model="ir.actions.act_window">
            <field name="name">Manufacturer</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">contact.lens.manufacturer</field>
            <field name="view_mode">tree,form</field>
        </record>


        <record id="erp_frames_action" model="ir.actions.act_window">
            <field name="name">Frames</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="domain">[('categ_id','=','Frames')]</field>
            <field name="context">{'def_categ_id':'Frames' }</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="erp_lens_action" model="ir.actions.act_window">
            <field name="name">Lens</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="domain">[('categ_id','=','Lens')]</field>
            <field name="context">{'def_categ_id':'Lens' }</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="erp_contactLens_action" model="ir.actions.act_window">
            <field name="name">Contact Lens</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="domain">[('categ_id','=','Contact Lens')]</field>
            <field name="context">{'def_categ_id':'Contact Lens' }</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="erp_treatment_action" model="ir.actions.act_window">
            <field name="name">Treatment</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="domain">[('categ_id','=','Lens Treatment')]</field>
            <field name="context">{'def_categ_id':'Lens Treatment' }</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="erp_service_action" model="ir.actions.act_window">
            <field name="name">Service</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="domain">[('categ_id','=','Service')]</field>
            <field name="context">{'def_categ_id':'Service' }</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="erp_miscellaneous_action" model="ir.actions.act_window">
            <field name="name">Miscellaneous</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product.template</field>
            <field name="domain">[('categ_id','=','Miscellaneous')]</field>
            <field name="context">{'def_categ_id':'Miscellaneous' }</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="product_frames" name="Frame" parent="optical_erp_config"
                  sequence="4"/>
        <menuitem id="product_lenses" name="Lens" parent="optical_erp_config"
                  sequence="10"/>
        <menuitem id="product_contact_lenses" name="Contact Lens" parent="optical_erp_config"
                  sequence="14"/>

        <menuitem id="product_frames_brand" name="Brands" parent="product_frames" action="frame_brand_action"
                  sequence="4"/>
        <menuitem id="product_frames_collection" name="Collections" parent="product_frames"
                  action="frame_collection_action"
                  sequence="5"/>
        <menuitem id="product_frames_type" name="Types" parent="product_frames"
                  action="frame_type_action"
                  sequence="6"/>
        <menuitem id="product_frames_material" name="Materials" parent="product_frames"
                  action="frame_material_action"
                  sequence="7"/>
        <menuitem id="product_frames_manufacturer" name="Manufacturer" parent="product_frames"
                  action="frame_manufacturer_action"
                  sequence="8"/>
        <menuitem id="product_frames_shape" name="Shape" parent="product_frames"
                  action="frame_shape_action"
                  sequence="9"/>

        <menuitem id="product_lenses_type" name="Type" parent="product_lenses" action="lens_type_action"
                  sequence="11"/>
        <menuitem id="product_lenses_style" name="Style" parent="product_lenses" action="lens_style_action"
                  sequence="12"/>
        <menuitem id="product_lenses_material" name="Material" parent="product_lenses" action="lens_material_action"
                  sequence="13"/>
        <menuitem id="product_contact_lenses_manufacturer" name="Manufacturer" parent="product_contact_lenses" action="contact_lens_manufacturer_action"
                  sequence="14"/>



        <menuitem id="optical_erp_products" name="Products" parent="optical_erp_root"
                  sequence="5"/>
        <menuitem id="optical_erp_product_frame" name="Frames" parent="optical_erp_products" action="erp_frames_action"
                  sequence="1"/>
        <menuitem id="optical_erp_product_lens" name="Lens" parent="optical_erp_products" action="erp_lens_action"
                  sequence="2"/>
        <menuitem id="optical_erp_product_contactLens" name="Contact Lens" parent="optical_erp_products"
                  action="erp_contactLens_action"
                  sequence="3"/>
        <menuitem id="optical_erp_product_treatment" name="Lens Treatment" parent="optical_erp_products"
                  action="erp_treatment_action"
                  sequence="4"/>
        <menuitem id="optical_erp_product_service" name="Service" parent="optical_erp_products"
                  action="erp_service_action"
                  sequence="5"/>
        <menuitem id="optical_erp_product_miscellaneous" name="Miscellaneous" parent="optical_erp_products"
                  action="erp_miscellaneous_action"
                  sequence="6"/>
    </data>
</odoo>
