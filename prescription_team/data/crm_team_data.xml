<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="team_prescription_department" model="crm.team">
            <field name="name">Prescription</field>
            <field name="sequence">0</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="crm_team_member_admin_prescription" model="crm.team.member" forcecreate="0">
            <field name="crm_team_id" ref="team_prescription_department"/>
            <field name="user_id" ref="base.user_admin"/>
        </record>

        <record id="prescription_team_website_prescription" model="crm.team">
            <field name="name">Website</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="False"/>
        </record>

        <record id="pos_prescription_team" model="crm.team">
            <field name="name">Point of Prescription</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="False"/>
        </record>

        <record id="ebay_prescription_team" model="crm.team">
            <field name="name">eBay</field>
            <field name="active" eval="False"/>
        </record>

        <function model="crm.team" name="message_unsubscribe"
            eval="[
            ref('prescription_team.team_prescription_department'),
            ref('prescription_team.prescription_team_website_prescription'),
            ref('prescription_team.ebay_prescription_team'),
            ref('prescription_team.pos_prescription_team')],
            [ref('base.partner_root')]"/>
    </data>
</odoo>
