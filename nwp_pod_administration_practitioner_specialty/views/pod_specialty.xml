<odoo>
    <record id="pod_specialty_view_form" model="ir.ui.view">
        <field name="name">pod.specialty.view.form</field>
        <field name="model">pod.specialty</field>
        <field
            name="inherit_id"
            ref="pod_administration_practitioner_specialty.pod_specialty_view_form"
        />
        <field name="arch" type="xml">
            <field name="description" position="after">
                <field name="code" />
                <label for="sequence_number_next" />
                <div>
                    <field
                        name="sequence_number_next"
                        class="oe_inline"
                        style="padding-right: 1.0em"
                    />
                    <field
                        name="sequence_id"
                        required="0"
                        class="oe_inline"
                        attrs="{'readonly': 1}"
                        groups="base.group_no_one"
                    />
                </div>
            </field>
        </field>
    </record>
</odoo>
