        <field name="name">Work Order</field>
        <field name="code">booking.work.order</field>
        <field name="prefix">WO</field>
        <field name="suffix"></field>
        <field name="padding">4</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
    </record> -->

        <record id="sequence_config_session" model="ir.sequence">
            <field name="name">Configuration Session</field>
            <field name="code">product.config.session</field>
            <field name="prefix">CS</field>
            <field name="padding">4</field>
        </record>



    </data>
</odoo>
