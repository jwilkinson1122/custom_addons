<odoo>
    <data noupdate="1">
        <!--sequence for prescription-->
        <record id="seq_prescription" model="ir.sequence">
            <field name="name">Prescription Sequence</field>
            <field name="code">kmhospital.prescription</field>
            <field name="prefix">%(y)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

