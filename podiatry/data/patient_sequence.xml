<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">

        <!-- Sequences For patient.code (Patient Code) -->

        <record id="seq_patient_code" model="ir.sequence">
            <field name="name">Patient code</field>
            <field name="code">patient.code</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences For patient.registration (Registration Code) -->

        <record id="seq_patient_reg" model="ir.sequence">
            <field name="name">Patient registration code</field>
            <field name="code">patient.registration</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences For patient.patient (Personal Identification Number) -->

        <record id="seq_patient_patient" model="ir.sequence">
            <field name="name">Patient</field>
            <field name="code">patient.patient</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences For patient.document (Patient Documents) -->

        <record id="seq_patient_document" model="ir.sequence">
            <field name="name">Document</field>
            <field name="code">patient.document</field>
            <field name="prefix">F</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences For document.type (Document Type) -->

        <record id="seq_patient_sequence" model="ir.sequence">
            <field name="name">sequence</field>
            <field name="code">document.type</field>
            <field name="padding">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
