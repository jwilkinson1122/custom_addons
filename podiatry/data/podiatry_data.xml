<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record forcecreate="True" id="ir_cron_prescription_costs_generator" model="ir.cron">
            <field name="name">Podiatry: Generate prescriptions costs based on costs frequency</field>
            <field name="model_id" ref="model_podiatry_device_log_prescription"/>
            <field name="state">code</field>
            <field name="code">model.run_scheduler()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>

        <record id="podiatry_device_state_new_request" model="podiatry.device.state">
            <field name="name">New Request</field>
            <field name="sequence">4</field>
        </record>

        <record id="podiatry_device_state_to_order" model="podiatry.device.state">
            <field name="name">To Order</field>
            <field name="sequence">5</field>
        </record>

        <record id="podiatry_device_state_registered" model="podiatry.device.state">
            <field name="name">Registered</field>
            <field name="sequence">7</field>
        </record>

        <record id="podiatry_device_state_downgraded" model="podiatry.device.state">
            <field name="name">Downgraded</field>
            <field name="sequence">8</field>
        </record>
    </data>
</odoo>
