<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="line_superglass" model="podiatry.device.model.line">
        <field name="name">Superglass</field>
        <field name="image_128" type="base64" file="podiatry/static/img/line_superglass-image.jpg"/>
    </record>
    <record id="line_ncv" model="podiatry.device.model.line">
        <field name="name">NCV</field>
        <field name="image_128" type="base64" file="podiatry/static/img/line_ncv-image.jpg"/>
    </record>
    <record id="line_prescription_comfort" model="podiatry.device.model.line">
        <field name="name">Prescription Comfort</field>
        <field name="image_128" type="base64" file="podiatry/static/img/line_prescription-comfort-image.jpg"/>
    </record>
    <record id="model_flex" model="podiatry.device.model">
        <field name="name">Flex</field>
        <field name="line_id" ref="line_superglass" />
        <field name="device_type">device</field>
    </record>
    <record id="model_everyday" model="podiatry.device.model">
        <field name="name">Everyday</field>
        <field name="line_id" ref="line_superglass" />
        <field name="device_type">device</field>
    </record>
    <record id="model_proformance" model="podiatry.device.model">
        <field name="name">Proformance</field>
        <field name="line_id" ref="line_superglass" />
        <field name="device_type">device</field>
    </record>
    <record id="model_gentle" model="podiatry.device.model">
        <field name="name">Gentle</field>
        <field name="line_id" ref="line_ncv" />
        <field name="device_type">device</field>
    </record>
    <record id="model_firm" model="podiatry.device.model">
        <field name="name">Firm</field>
        <field name="line_id" ref="line_ncv" />
        <field name="device_type">device</field>
    </record>
    <record id="model_multi_density" model="podiatry.device.model">
        <field name="name">Multi-Density</field>
        <field name="line_id" ref="line_prescription_comfort" />
        <field name="device_type">device</field>
    </record>
    <record id="model_composite" model="podiatry.device.model">
        <field name="name">Composite</field>
        <field name="line_id" ref="line_prescription_comfort" />
        <field name="device_type">device</field>
    </record>
</odoo>
