<?xml version="1.0" encoding="utf-8"?>

<odoo>
        <record id="report_student_student" model="ir.actions.report">
        <field name="name">Identity Card</field>
        <field name="model">student.student</field>
        <field name="binding_model_id" ref="model_student_student"/>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">podiatry.identity_card</field>
        <field name="report_file">podiatry.identity_card</field>
        <field name="binding_type">report</field>
        <field name="groups_id" eval="[(4, ref('podiatry.group_podiatry_administration')),
                                       (4, ref('podiatry.group_podiatry_teacher')),
                                       (4, ref('podiatry.group_podiatry_student'))]"/>
    </record>
        <record id="report_podiatry_identity_card" model="ir.actions.report">
        <field name="name">Identity Card</field>
        <field name="model">podiatry.teacher</field>
        <field name="binding_model_id" ref="model_podiatry_teacher"/>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">podiatry.teacher_identity_card</field>
        <field name="report_file">podiatry.teacher_identity_card</field>
        <field name="binding_type">report</field>
    </record>
</odoo>
