            <!-- Dress -->

                    <!-- Heel below 1.5" -->
                        <!-- Superglass Everyday shell OR Polypropylene Semi-rigid shell / black microsuede top
                        cover (to sulcus) / 1/16” P-Foam extension (sulcus-length) / X-Guard(standard) -->
                    

                    <!-- Heel above 1.5" -->
                        <!-- Superglass High Heel shell / leather top cover (to sulcus) / 1/16” P-Foam extension
