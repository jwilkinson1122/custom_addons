<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="podiatry_puerperium_monitor_tree_view" model="ir.ui.view">
            <field name="name">podiatry.puerperium.monitor.tree.view</field>
            <field name="model">podiatry.puerperium.monitor</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="date" />
                    <field name="systolic_pressure" />
                    <field name="diastolic_pressure" />
                    <field name="heart_freq" />
                    <field name="temprature" />
                    <field name="fundal_height" />
                    <field name="lochia_amount" />
                    <field name="lochia_color" />
                    <field name="loicha_order" />
                </tree>
            </field>
        </record>

</odoo>
