<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Forefoot Values -->
    <record id="podiatry_forefoot_value_form" model="ir.ui.view">
        <field name="name">podiatry.forefoot.value.form</field>
        <field name="model">podiatry.forefoot.value</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="sequence"/>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="note"/>
                    </group>
                </group>
            </form>
        </field>
    </record>
    <record id="podiatry_forefoot_value_tree" model="ir.ui.view">
        <field name="name">podiatry.forefoot.value.tree</field>
        <field name="model">podiatry.forefoot.value</field>
        <field name="arch" type="xml">
            <tree>
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="note"/>
            </tree>
        </field>
    </record>
    <!--Action Forefoot Values -->
    <record id="forefoot_values_action" model="ir.actions.act_window">
        <field name="res_model">podiatry.forefoot.value</field>
        <field name="view_mode">tree,form</field>
    </record>

