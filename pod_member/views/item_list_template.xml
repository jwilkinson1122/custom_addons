<odoo>
  <template id="item_list_extended" name="Extended Item List" inherit_id="pod_app.item_list_template">

    <xpath expr="//span[@t-field='item.publisher_id']" position="after">
      <t t-if="not item.is_available">
        <b>(Not Available)</b>
      </t>
    </xpath>

  </template>
</odoo>
