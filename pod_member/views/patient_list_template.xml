<odoo>
  <template id="patient_list_extended" name="Extended Patient List" inherit_id="pod_app.patient_list_template">

    <xpath expr="//span[@t-field='patient.publisher_id']" position="after">
      <t t-if="not patient.is_available">
        <b>(Not Available)</b>
      </t>
    </xpath>

  </template>
</odoo>
