<odoo>
    <record id="product_brand_sg" model="product.brand">
        <field name="name">Superglass</field>
    </record>
    <record id="product_template_vinyl_top_cover" model="product.template" context="{'create_product_product': False}">
        <field name="name">Vinyl Top Cover</field>
        <field name="categ_id" ref="product_category_top_covers"/>
        <field name="available_in_pos" eval="True"/>
        <!-- <field name="allow_measurements" eval="True"/> -->
        <field name="is_custom_product" eval="True"/>
        <field name="purchase_ok" eval="False"/>
        <field name="pos_categ_ids" eval="[(6, 0, [ref('pos_product_category_top_covers')])]"/>
        <field name="detailed_type">product</field>
        <field name="uom_id" ref="uom.product_uom_unit" />
        <field name="standard_price">3.95</field>
        <field name="list_price">3.95</field>
        <field name="image_1920" type="base64" file="nwpl_pod_master/static/pod_pos_order_customizations/static/src/img/top_covers/vinyl.webp"/>
        <!-- <field name="has_configurable_attributes" eval="True"/> -->
        <field name="available_in_pos" eval="True"/>
        <!-- <field name="no_create_variants">yes</field> -->
    </record>


