        <!-- Sequence -->
        <record id="seq_prescription_location" model="ir.sequence">
            <field name="name">Prescription Location Sequence</field>
            <field name="code">prescription.location</field>
            <field name="prefix">PL</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
        </record>

        <!--This record for adding sequence to prescription.order-->
        <record id="sequence_prescription_order" model="ir.sequence">
            <field name="name">Prescription Order</field>
            <field name="code">prescription.order</field>
            <field name="prefix">RX</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>



    </data>

</odoo>