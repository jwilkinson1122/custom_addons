<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


<!-- data files -->

        <record id="prescriptions_sign_cla" model="prescriptions.prescription">
            <field name="name">Odoo CLA.pdf</field>
            <field name="type">binary</field>
            <field name="attachment_id" ref="sign.attachment_sign_3"/>
            <field name="folder_id" ref="prescriptions.prescriptions_internal_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_internal_status_validated'),
                                               ref('prescriptions.prescriptions_internal_template_contracts'),
                                               ref('prescriptions.prescriptions_internal_knowledge_hr')])]"/>
        </record>

        <record id="prescriptions_sign_employment" model="prescriptions.prescription">
            <field name="name">Employment Contract.pdf</field>
            <field name="type">binary</field>
            <field name="attachment_id" ref="sign.attachment_sign_1"/>
            <field name="folder_id" ref="prescriptions.prescriptions_internal_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_internal_status_validated'),
                                               ref('prescriptions.prescriptions_internal_template_contracts'),
                                               ref('prescriptions.prescriptions_internal_knowledge_hr')])]"/>
        </record>

        <record id="prescriptions_attachment_video_studio_prescription" model="prescriptions.prescription">
            <field name="name">sample-nda.pdf</field>
            <field name="type">binary</field>
            <field name="attachment_id" ref="sign.attachment_sign_2"/>
            <field name="folder_id" ref="prescriptions.prescriptions_internal_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_internal_template_contracts')])]"/>
        </record>

    </data>
</odoo>
