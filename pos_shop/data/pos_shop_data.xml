<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="pos_config_main_shop" model="pos.config">
            <field name="name">Manufacturing Shop</field>
            <field name="module_pos_shop">True</field>
            <field name="iface_splitinvoice">True</field>
            <field name="iface_printinvoice">False</field>
            <field name="iface_orderline_notes">True</field>
            <field name="iface_tipproduct">False</field>
            <field name="start_category">False</field>
            <field name="limit_categories">False</field>
        </record>

        <function model="pos.config" name="_setup_main_shop_defaults">
            <value eval="[ref('pos_config_main_shop')]"/>
        </function>
    </data>
</odoo>
