<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Sequences for hotel folio -->
    <record id="seq_hotel_folio" model="ir.sequence">
        <field name="name">Hotel Folio</field>
        <field name="code">hotel.folio</field>
        <field name="prefix">Folio/</field>
        <field name="padding">5</field>
    </record>
    <data noupdate="1">
        <record id="seq_config_session" model="ir.sequence">
            <field name="name">Configuration Session</field>
            <field name="code">product.config.session</field>
            <field name="prefix">CS</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
