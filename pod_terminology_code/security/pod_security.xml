<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="group_code_concept_display" model="res.groups">
        <field name="name">Podiatry Terminology Concept Display</field>
        <field name="category_id" ref="base.module_category_hidden" />
    </record>
    <record id="group_code_concept_list" model="res.groups">
        <field name="name">Podiatry Terminology Concept List</field>
        <field name="category_id" ref="base.module_category_hidden" />
        <field name="implied_ids" eval="[(4, ref('group_code_concept_display')), (4, ref('pod_base.group_pod_root_menu')), (4, ref('pod_base.group_pod_terminologies_menu'))]" />
    </record>
    <!-- Generic groups -->
    <record id="pod_base.group_pod_user" model="res.groups">
        <field name="implied_ids" eval="[(4, ref('group_code_concept_display'))]" />
    </record>
</odoo>
