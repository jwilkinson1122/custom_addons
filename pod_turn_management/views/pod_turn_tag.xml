<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="pod_turn_tag_form_view">
        <field name="name">pod.turn.tag.form (in pod_turn_management)</field>
        <field name="model">pod.turn.tag</field>
        <field name="arch" type="xml">
            <form>
                <header>
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
                <div class="oe_chatter" />
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="pod_turn_tag_tree_view">
        <field name="name">pod.turn.tag.tree (in pod_turn_management)</field>
        <field name="model">pod.turn.tag</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="pod_turn_tag_act_window">
        <field name="name">Podiatry Turn Tag</field>
        <field name="res_model">pod.turn.tag</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.ui.menu" id="pod_turn_tag_menu">
        <field name="name">Podiatry Turn Tag</field>
        <field name="parent_id" ref="pod_turn_root" />
        <field name="action" ref="pod_turn_tag_act_window" />
        <field name="sequence" eval="100" />
    </record>
</odoo>
