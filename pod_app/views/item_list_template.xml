<odoo>

  <template id="item_list_template" name="Item List">
    <div id="wrap" class="container">
      <h1>Items</h1>
      <t t-foreach="items" t-as="item">
        <div class="row">
          <span t-field="item.name" />
,
          <span t-field="item.date_published" />
,
          <span t-field="item.publisher_id" />
        </div>
      </t>
    </div>
  </template>

</odoo>
