<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Action of account practice -->
    <record id="open_account_practice_form_tree" model="ir.actions.act_window">
        <field name="name">Practice</field>
        <field name="res_model">account.practice</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- Action for account device type -->
    <record id="open_account_device_type_form_tree" model="ir.actions.act_window">
        <field name="name">Device Type</field>
        <field name="res_model">account.device.type</field>
        <field name="context">{}</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- Action for account device customization type -->
    <record id="action_account_device_customization_type_view_form" model="ir.actions.act_window">
        <field name="name">Customization Type</field>
        <field name="res_model">account.device.customization.type</field>
        <field name="context">{}</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- Action for account device customization -->
    <record id="action_account_device_customization_view_form" model="ir.actions.act_window">
        <field name="name">Customization</field>
        <field name="res_model">account.device.customization</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'default_is_cate_gid':1,'default_available_in_pos':0}
        </field>
        <field name="view_id" ref="view_account_device_customization_list" />
    </record>
    <!-- Action for account device -->
    <record id="action_account_device_form" model="ir.actions.act_window">
        <field name="name">Device</field>
        <field name="res_model">account.device</field>
        <field name="context">{'default_is_device':1,'default_rental':1,'default_available_in_pos':0}
        </field>
        <field name="view_id" ref="view_account_device_tree" />
        <field name="view_mode">kanban,tree,form</field>
    </record>
    <!-- Action for account prescription -->
    <record id="open_account_prescription1_form_tree_all" model="ir.actions.act_window">
        <field name="name">Generate Prescription</field>
        <field name="res_model">account.prescription</field>
        <field name="view_mode">tree,form,calendar,graph</field>
        <field name="context">{'prescription': True}</field>
    </record>
    <!-- Action for account service type -->
    <record id="open_account_service_type_form_tree" model="ir.actions.act_window">
        <field name="name">Service Type</field>
        <field name="res_model">account.service.type</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- Action for account service -->
    <record id="action_account_services_form" model="ir.actions.act_window">
        <field name="name">Services</field>
        <field name="res_model">account.services</field>
        <field name="context">{'default_is_service':1,'default_available_in_pos':0}
        </field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
