<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_pod">
        <field name="name">Podiatry Management</field>
        <field name="description">Category For Pod</field>
        <field name="sequence">45</field>
    </record>

    <record id="group_pod_labman" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_pod"/>
    </record>

    <record id="group_pod_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="module_category_pod"/>
        <field name="implied_ids" eval="[(4, ref('group_pod_labman'))]"/>
    </record>

</odoo>