<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_practice">
        <field name="name">Practice Management</field>
        <field name="description">Category For Practice</field>
        <field name="sequence">45</field>
    </record>

    <record id="group_practice_labman" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_practice"/>
    </record>

    <record id="group_practice_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="module_category_practice"/>
        <field name="implied_ids" eval="[(4, ref('group_practice_labman'))]"/>
    </record>

</odoo>