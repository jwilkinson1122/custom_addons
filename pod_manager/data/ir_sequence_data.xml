<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="imaging_sequence" model="ir.sequence">
            <field name="name">Imaging Request</field>
            <field name="code">imaging_seq</field>
            <field name="prefix">IMG</field>
            <field name="padding">3</field>
        </record>

        <record id="pod_health_service_code_sequence" model="ir.sequence">
            <field name="name">Service</field>
            <field name="code">pod.health_service</field>
            <field name="prefix">SERV%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="pod_health_service_sequence" model="ir.sequence">
            <field name="name">Podiatry Health Service</field>
            <field name="code">pod_health_ser</field>
            <field name="prefix">MEDSR</field>
            <field name="padding">3</field>
        </record>

        <record id="rx_invoice_sequence" model="ir.sequence">
            <field name="name">Rx Order Invoice</field>
            <field name="code">pres_inv_seq</field>
            <field name="prefix">PRES/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="lab_invoice_sequence" model="ir.sequence">
            <field name="name">Lab Invoice</field>
            <field name="code">lab_inv_seq</field>
            <field name="prefix">LAB/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="pod_health_service_invoice_sequence" model="ir.sequence">
            <field name="name">Podiatry Health Service Invoice</field>
            <field name="code">pod_health_service_inv_seq</field>
            <field name="prefix">MED/HS/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="pod_lab_test_invoice_sequence" model="ir.sequence">
            <field name="name">Podiatry Lab Test Invoice</field>
            <field name="code">pod_lab_test_inv_seq</field>
            <field name="prefix">MED/LT/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="patient_sequence" model="ir.sequence">
            <field name="name">Patients</field>
            <field name="code">pod.patient</field>
            <field name="prefix">PID</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="rx_order_sequence" model="ir.sequence">
            <field name="name">Prescriptions</field>
            <field name="code">pod.rx.order</field>
            <field name="prefix">PRES%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
