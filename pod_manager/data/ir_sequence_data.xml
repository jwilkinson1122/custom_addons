<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="pod_order_sequence" model="ir.sequence">
            <field name="name"> Orders</field>
            <field name="code">pod.order</field>
            <field name="prefix">PRES%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="pod_order_invoice_sequence" model="ir.sequence">
            <field name="name"> Order Invoice</field>
            <field name="code">pres_inv_seq</field>
            <field name="prefix">PRES/INV/</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
