<odoo>
    <record id="podiatry_patient_view_form" model="ir.ui.view">
        <field name="name">podiatry.patient.form</field>
        <field name="model">podiatry.patient</field>
        <field name="inherit_id" ref="podiatry_base.podiatry_patient_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='phone']" position="before">


                <!-- <label for="podiatry_location_primary_id" /> -->
                <field name="podiatry_location_primary_id" />
                <!-- <label for="podiatry_location_secondary_ids" /> -->
                <field name="podiatry_location_secondary_ids" widget="many2many_tags" />

            </xpath>
        </field>
    </record>
</odoo>
