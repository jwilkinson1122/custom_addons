<?xml version="1.0" encoding="utf-8" ?>

<odoo noupdate="1">

    <record id="partner_type_doctor" model="partner.type">
      <field name="name">Doctor</field>
      <field name="code">DOCTOR</field>
      <field name="shortcut">DPM</field>
      <field name="doctor" eval="True"/>
      <field name="receptionist" eval="False"/>
      <field name="nurse" eval="False"/>
      <field name="administrator" eval="False"/>
      <field name="patient" eval="False"/>
      <field name="other" eval="False"/>
    </record>

    <record id="partner_type_receptionist" model="partner.type">
      <field name="name">Receptionist</field>
      <field name="code">RECEPTIONIST</field>
      <field name="shortcut">REC</field>
      <field name="doctor" eval="False"/>
      <field name="receptionist" eval="True"/>
      <field name="nurse" eval="False"/>
      <field name="administrator" eval="False"/>
      <field name="patient" eval="False"/>
      <field name="other" eval="False"/>
    </record>

    <record id="partner_type_administrator" model="partner.type">
      <field name="name">Administrator</field>
      <field name="code">ADMINISTRATOR</field>
      <field name="shortcut">ADM</field>
      <field name="doctor" eval="False"/>
      <field name="receptionist" eval="False"/>
      <field name="nurse" eval="False"/>
      <field name="administrator" eval="True"/>
      <field name="patient" eval="False"/>
      <field name="other" eval="False"/>
    </record>

    <record id="partner_type_nurse" model="partner.type">
      <field name="name">Nurse</field>
      <field name="code">NURSE</field>
      <field name="shortcut">NUR</field>
      <field name="doctor" eval="False"/>
      <field name="receptionist" eval="False"/>
      <field name="nurse" eval="True"/>
      <field name="administrator" eval="False"/>
      <field name="patient" eval="False"/>
      <field name="other" eval="False"/>
    </record>
     
    <record id="partner_type_patient" model="partner.type">
      <field name="name">Patient</field>
      <field name="code">PATIENT</field>
      <field name="shortcut">PAT</field>
      <field name="doctor" eval="False"/>
      <field name="receptionist" eval="False"/>
      <field name="nurse" eval="False"/>
      <field name="administrator" eval="False"/>
      <field name="patient" eval="True"/>
      <field name="other" eval="False"/>
    </record>

    <record id="partner_type_other" model="partner.type">
      <field name="name">Other</field>
      <field name="code">OTHER</field>
      <field name="shortcut">OTH</field>
      <field name="doctor" eval="False"/>
      <field name="receptionist" eval="False"/>
      <field name="nurse" eval="False"/>
      <field name="administrator" eval="False"/>
      <field name="patient" eval="False"/>
      <field name="other" eval="True"/>
    </record>

</odoo>
