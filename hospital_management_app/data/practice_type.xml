<?xml version="1.0" encoding="utf-8" ?>

<odoo noupdate="1">

    <record id="practice_type_clinic" model="practice.type">
      <field name="name">Clinic</field>
      <field name="code">CLINIC</field>
      <field name="shortcut">CLN</field>
      <field name="clinic" eval="True"/>
      <field name="hospital" eval="False"/>
      <field name="military" eval="False"/>
      <field name="other" eval="False"/>
    </record>

    <record id="practice_type_hospital" model="practice.type">
      <field name="name">Hospital</field>
      <field name="code">HOSPITAL</field>
      <field name="shortcut">HSP</field>
      <field name="clinic" eval="False"/>
      <field name="hospital" eval="True"/>
      <field name="military" eval="False"/>
      <field name="other" eval="False"/>
    </record>

    <record id="practice_type_military" model="practice.type">
      <field name="name">Military</field>
      <field name="code">MILITARY</field>
      <field name="shortcut">MVA</field>
      <field name="clinic" eval="False"/>
      <field name="hospital" eval="False"/>
      <field name="military" eval="True"/>
      <field name="other" eval="False"/>
    </record>

    <record id="practice_type_other" model="practice.type">
      <field name="name">Other</field>
      <field name="code">OTHER</field>
      <field name="shortcut">OTH</field>
      <field name="clinic" eval="False"/>
      <field name="hospital" eval="False"/>
      <field name="military" eval="False"/>
      <field name="other" eval="True"/>
    </record>

</odoo>