<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
     <data noupdate="1">

          <!--        Paperformat-->
          <record id="paperformat_optical_prescription" model="report.paperformat">
               <field name="name">Optical Prescription Paperformat A4 (Fit)</field>
               <field name="default" eval="True" />
               <field name="format">A4</field>
               <field name="page_height">0</field>
               <field name="page_width">0</field>
               <field name="orientation">Portrait</field>
               <field name="margin_top">35</field>
               <field name="margin_bottom">28</field>
               <field name="margin_left">0</field>
               <field name="margin_right">0</field>
               <field name="header_line" eval="False" />
               <field name="header_spacing">35</field>
               <field name="dpi">90</field>
          </record>

          <!--Designs-->
          <record id="data_reporting_custom_template__optical_prescription_default" model="report.custom.template.template">
