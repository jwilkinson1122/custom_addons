            </field>
        </field>
    </record>
    <record model="ir.ui.view" id="safe_box_coin_tree">
        <field name="name">safe.box.coin.tree</field>
        <field name="model">safe.box.coin</field>
        <field name="inherit_id" ref="safe_box.safe_box_coin_tree" />
        <field name="arch" type="xml">
            <field name="rate" position="before">
                <field name="type" />
            </field>
        </field>
    </record>
</odoo>
