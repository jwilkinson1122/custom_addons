<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- prescriptions search view -->
    <record id="prescription_view_search_inherit_prescriptions_product" model="ir.ui.view">
        <field name="name">prescriptions.view.search.inherit.prescriptions_product</field>
        <field name="model">prescriptions.prescription</field>
        <field name="inherit_id" ref="prescriptions.prescription_view_search"/>
        <field name="arch" type="xml">
            <field name="type" position="after">
                <field name="product_template_id"/>
                <field name="product_id"/>
            </field>
        </field>
    </record>
</odoo>
