<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="team_prescriptions_department" model="crm.team">
            <field name="name">Prescriptions</field>
            <field name="sequence">0</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="crm_team_member_admin_prescriptions" model="crm.team.member" forcecreate="0">
            <field name="crm_team_id" ref="team_prescriptions_department"/>
            <field name="user_id" ref="base.user_admin"/>
        </record>

        <record id="prescriptions_team_website_prescriptions" model="crm.team">
            <field name="name">Website</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="False"/>
        </record>

        <record id="pos_prescriptions_team" model="crm.team">
            <field name="name">Point of Prescription</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="False"/>
        </record>

        <record id="ebay_prescriptions_team" model="crm.team">
            <field name="name">eBay</field>
            <field name="active" eval="False"/>
        </record>

        <function model="crm.team" name="message_unsubscribe"
            eval="[
            ref('pod_prescriptions_team.team_prescriptions_department'),
            ref('pod_prescriptions_team.prescriptions_team_website_prescriptions'),
            ref('pod_prescriptions_team.ebay_prescriptions_team'),
            ref('pod_prescriptions_team.pos_prescriptions_team')],
            [ref('base.partner_root')]"/>
    </data>
</odoo>
