<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="seq_prescription_partner" model="ir.sequence">
        <field name="name">Partner sequence</field>
        <field name="code">partner.internal.code</field>
        <field name="prefix"></field>
        <field name="padding">4</field>
    </record>
    <record id="seq_prescription_practice" model="ir.sequence">
        <field name="name">Practice sequence</field>
        <field name="code">prescription.practice</field>
        <field name="prefix">MP</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_prescription_location" model="ir.sequence">
        <field name="name">Location sequence</field>
        <field name="code">prescription.location</field>
        <field name="prefix">ML</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_prescription_practitioner" model="ir.sequence">
        <field name="name">Practitioner sequence</field>
        <field name="code">prescription.practitioner</field>
        <field name="prefix">PT</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_prescription_patient" model="ir.sequence">
        <field name="name">Patient sequence</field>
        <field name="code">prescription.patient</field>
        <field name="prefix">PID</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_prescription_flag" model="ir.sequence">
        <field name="name">Flag sequence</field>
        <field name="code">prescription.flag</field>
        <field name="prefix">FL</field>
        <field name="padding">6</field>
    </record>
</odoo>
