<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="group_podiatry_sct_concept_display" model="res.groups">
        <field name="name">Podiatry SCT Concept Display</field>
        <field name="category_id" ref="base.module_category_hidden" />
    </record>
    <record id="group_podiatry_sct_concept_list" model="res.groups">
        <field name="name">Podiatry SCT Concept List</field>
        <field name="category_id" ref="base.module_category_hidden" />
        <field name="implied_ids" eval="[(4, ref('group_podiatry_sct_concept_display')), (4, ref('podiatry_base.group_podiatry_root_menu')), (4, ref('podiatry_base.group_podiatry_terminologies_menu'))]" />
    </record>
    <!-- Generic groups -->
    <record id="podiatry_base.group_podiatry_user" model="res.groups">
        <field name="implied_ids" eval="[(4, ref('group_podiatry_sct_concept_display'))]" />
    </record>
</odoo>
