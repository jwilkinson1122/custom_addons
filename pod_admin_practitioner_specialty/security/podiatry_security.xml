<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="group_podiatry_specialty_display" model="res.groups">
        <field name="name">Specialty Display</field>
        <field name="category_id" ref="base.module_category_hidden" />
    </record>
    <record id="group_podiatry_specialty_list" model="res.groups">
        <field name="name">Specialty List</field>
        <field name="category_id" ref="base.module_category_hidden" />
        <field name="implied_ids" eval="[(4, ref('group_podiatry_specialty_display'))]" />
    </record>
    <record id="group_podiatry_specialty_manager" model="res.groups">
        <field name="name">Specialty Manager</field>
        <field name="category_id" ref="base.module_category_hidden" />
        <field name="implied_ids" eval="[(4, ref('group_podiatry_specialty_list'))]" />
    </record>
    <record id="podiatry_administration_practitioner.group_podiatry_practitioner_display" model="res.groups">
        <field name="implied_ids" eval="[(4, ref('podiatry_administration_practitioner_specialty.group_podiatry_specialty_display'))]" />
    </record>
    <record id="podiatry_administration_practitioner.group_podiatry_practitioner_manager" model="res.groups">
        <field name="implied_ids" eval="[(4, ref('podiatry_administration_practitioner_specialty.group_podiatry_specialty_manager'))]" />
    </record>
</odoo>
