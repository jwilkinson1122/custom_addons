<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="patient_sequence" model="ir.sequence">
            <field name="name">Patient ID</field>
            <field name="code">patient</field>
            <field name="prefix">PID</field>
            <field name="padding">6</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <!-- <field name="company_id" eval="False"/> -->
        </record>
        <!--sequence for practice-->
        <record id="practice_sequence" model="ir.sequence">
            <field name="name">Practice ID</field>
            <field name="code">practice</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        <!--sequence for practitioner-->
        <record id="practitioner_sequence" model="ir.sequence">
            <field name="name">Practitioner ID</field>
            <field name="code">practitioner</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>

        <record id="work_order_sequence" model="ir.sequence">
            <field name="name">Work Order</field>
            <field name="code">work.order</field>
            <field name="prefix">WO</field>
            <field name="suffix"></field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>

    </data>

</odoo>