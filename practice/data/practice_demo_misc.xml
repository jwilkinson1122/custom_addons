        </record>
        <record id="practice_tag_category_2_tag_3" model="practice.tag">
            <field name="name">Sport</field>
            <field name="sequence">12</field>
            <field name="category_id" ref="practice_tag_category_2"/>
            <field name="color">7</field>
        </record>

        <record id="practice_tag_category_3_tag_1" model="practice.tag">
            <field name="name">Online</field>
            <field name="sequence">20</field>
            <field name="category_id" ref="practice_tag_category_3"/>
            <field name="color">8</field>
        </record>
        <record id="practice_tag_category_3_tag_2" model="practice.tag">
            <field name="name">Conference</field>
            <field name="sequence">21</field>
            <field name="category_id" ref="practice_tag_category_3"/>
            <field name="color">9</field>
        </record>

    </data>
</odoo>
