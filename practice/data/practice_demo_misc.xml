<?xml version="1.0"?>
<odoo>
    <data>

        <!-- Practice Type -->
        <record id="practice_type_0" model="practice.type">
            <field name="name">Exhibition</field>
            <field name="auto_confirm" eval="False"/>
        </record>
        <record id="practice_type_1" model="practice.type">
            <field name="name">Training</field>
            <field name="auto_confirm" eval="False"/>
        </record>
        <record id="practice_type_2" model="practice.type">
            <field name="name">Sport</field>
            <field name="auto_confirm" eval="False"/>
            <field name="default_timezone">US/Pacific</field>
        </record>
        <record id="practice_type_data_conference" model="practice.type">
            <field name="default_timezone">Europe/Brussels</field>
        </record>

        <!-- Category and Tags -->
        <record id="practice_tag_category_1" model="practice.tag.category">
            <field name="name">Age</field>
            <field name="sequence">3</field>
        </record>
        <record id="practice_tag_category_2" model="practice.tag.category">
            <field name="name">Activity</field>
            <field name="sequence">1</field>
        </record>
        <record id="practice_tag_category_3" model="practice.tag.category">
            <field name="name">Type</field>
            <field name="sequence">2</field>
        </record>

        <record id="practice_tag_category_1_tag_1" model="practice.tag">
            <field name="name">5-10</field>
            <field name="sequence">1</field>
            <field name="category_id" ref="practice_tag_category_1"/>
            <field name="color">1</field>
        </record>

        <record id="practice_tag_category_1_tag_2" model="practice.tag">
            <field name="name">10-14</field>
            <field name="sequence">2</field>
            <field name="category_id" ref="practice_tag_category_1"/>
            <field name="color">2</field>
        </record>

        <record id="practice_tag_category_1_tag_3" model="practice.tag">
            <field name="name">15-18</field>
            <field name="sequence">3</field>
            <field name="category_id" ref="practice_tag_category_1"/>
            <field name="color">3</field>
        </record>

        <record id="practice_tag_category_1_tag_4" model="practice.tag">
            <field name="name">18+</field>
            <field name="sequence">4</field>
            <field name="category_id" ref="practice_tag_category_1"/>
