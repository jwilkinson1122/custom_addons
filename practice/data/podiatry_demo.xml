        <!--Practice-->

        <record id="dep_management" model="podiatry.practice">
            <field name="name">Management</field>
        </record>

        <!--Jobs-->

        <record id="job_physician" model="podiatry.job">
            <field name="name">Physician</field>
            <field name="practice_id" ref="dep_management"/>
        </record>

        <record id="job_assistant" model="podiatry.job">
            <field name="name">Assistant</field>
            <field name="practice_id" ref="dep_management"/>
        </record>

        <record id="job_manager" model="podiatry.job">
            <field name="name">Practice Manager</field>
            <field name="practice_id" ref="dep_administration"/>
