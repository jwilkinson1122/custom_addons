<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Practice Categories -->
        <record id="practice_type_data_low_profile_shoes" model="practice.type">
            <field name="name">Low Profile Shoes</field>
            <field name="auto_confirm" eval="False"/>
        </record>
        <record id="practice_type_data_conference" model="practice.type">
            <field name="name">Conference</field>
            <field name="auto_confirm" eval="True"/>
        </record>

        <!-- Practice stages -->
        <record id="practice_stage_new" model="practice.stage">
            <field name="name">New</field>
            <field name="description">Freshly created</field>
            <field name="sequence">1</field>
        </record>
        <record id="practice_stage_booked" model="practice.stage">
            <field name="name">Booked</field>
            <field name="description">The place has been reserved</field>
            <field name="sequence">2</field>
        </record>
        <record id="practice_stage_announced" model="practice.stage">
            <field name="name">Announced</field>
            <field name="description">The practice has been publicly announced</field>
            <field name="sequence">3</field>
        </record>
        <record id="practice_stage_done" model="practice.stage">
            <field name="name">Ended</field>
            <field name="description">Fully ended</field>
            <field name="sequence">5</field>
            <field name="pipe_end" eval="True"/>
            <field name="fold" eval="True"/>
        </record>
        <record id="practice_stage_cancelled" model="practice.stage">
            <field name="name">Cancelled</field>
            <field name="description">The practice has been cancelled</field>
            <field name="sequence">6</field>
            <field name="pipe_end" eval="True"/>
            <field name="fold" eval="True"/>
        </record>
        <!--sequence for patient-->
        <record id="sequence_podiatry_patient" model="ir.sequence">
            <field name="name">Patient ID</field>
            <field name="code">podiatry.patient</field>
            <field name="prefix">PID</field>
            <field name="padding">6</field>
            <!-- <field name="company_id" eval="False"/> -->
        </record>
        <!--sequence for practice-->
        <record id="sequence_podiatry_practice" model="ir.sequence">
            <field name="name">Practice ID</field>
            <field name="code">podiatry.practice</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
        </record>
        <!--sequence for practitioner-->
        <record id="sequence_podiatry_practitioner" model="ir.sequence">
            <field name="name">Practitioner ID</field>
            <field name="code">podiatry.practitioner</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
        </record>
        <!--sequence for practice-->
        <record id="sequence_practice_practice" model="ir.sequence">
            <field name="name">Practice Sequence</field>
            <field name="code">practice.practice</field>
            <field name="prefix">%(y)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <!--sequence for practice-->
        <record id="practice_invoice_sequence" model="ir.sequence">
            <field name="name">Practice Invoice</field>
            <field name="code">pres_inv_seq</field>
            <field name="prefix">PRES/INV/</field>
            <field name="padding">4</field>
        </record>


    </data>
</odoo>
