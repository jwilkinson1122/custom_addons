<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!--Group for practice user -->
    <record id="group_practice_user" model="res.groups">
        <field name="name">Practice Management / User</field>
        <field eval="[(4, ref('base.user_root'))]" name="implied_ids" />
    </record>
    <!--Group for practice manager -->
    <record id="group_practice_manager" model="res.groups">
        <field name="name">Practice Management/ Manager</field>
        <field eval="[(4, ref('practice.group_practice_user'))]" name="implied_ids" />
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
    </record>

</odoo>
