<odoo>
    <data>
        <record id="payment_method_izi" model="pos.payment.method">
            <field name="name">Izi</field>
            <field name="receivable_account_id" ref="account.data_account_type_receivable"/>
            <field name="is_cash_count" eval="False"/>
            <field name="split_transactions" eval="False"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="journal_id" ref="izi_pos.journal_izi"/>
        </record>
        <record id="payment_method_phone" model="pos.payment.method">
            <field name="name">Yape / Plin / Tunki</field>
            <field name="receivable_account_id" ref="account.data_account_type_receivable"/>
            <field name="is_cash_count" eval="False"/>
            <field name="split_transactions" eval="False"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="journal_id" ref="izi_pos.journal_phone"/>

        </record>
    </data>
</odoo>