<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="pos_config_custom_orthotics" model="pos.config">
        <field name="name">Booking Orders</field>
         <field name="iface_tipproduct" eval="True"/>
        <field name="use_pricelist" eval="1"/>
        <field name="iface_print_auto" eval="True"/>
        <field name="ship_later" eval="True"/>
        <field name="payment_method_ids" eval="[(6, 0, [
        ref('pos_payment_method_1'),
        ref('pos_payment_method_2'),
        ])]"/>
