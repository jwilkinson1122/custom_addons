<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="0">
        <record id="group_custom_orthotics_practice" model="res.groups">
            <field name="name">Practice</field>
            <field name="category_id" ref="base.module_category_medical_device_manufacturing" />
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
        </record>
        <record id="group_custom_orthotics_receptionist" model="res.groups">
            <field name="name">Receptionist</field>
            <field name="category_id" ref="base.module_category_medical_device_manufacturing" />
            <field name="implied_ids" eval="[(4, ref('group_custom_orthotics_practice'))]" />
        </record>
        <record id="group_custom_orthotics_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="base.module_category_medical_device_manufacturing" />
            <field name="implied_ids" eval="[(4, ref('group_custom_orthotics_receptionist'))]" />
            <field name="users" eval="[(4, ref('base.user_root'))]" />
        </record>
    </data>
    <data noupdate="1">
        <record id="base.default_user" model="res.users">
            <field name="groups_id" eval="[(4,ref('custom_orthotics.group_custom_orthotics_manager'))]" />
        </record>
    </data>
</odoo>