        <template id="assets_for_pos_product_operations" inherit_id="point_of_sale.assets">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/podiatry_pos_modifier/static/src/css/modifier.css" />
                <script type="text/javascript" src="/podiatry_pos_modifier/static/src/js/BiProductScreen.js"/>
