                <script type="text/javascript" src="/podiatry_pos_modifier/static/src/js/ProductTemplatePopupWidget.js"/>
                <script type="text/javascript" src="/podiatry_pos_modifier/static/src/js/screens.js"/>
                <script type="text/javascript" src="/podiatry_pos_modifier/static/src/js/BiProductList.js"/>
                <script type="text/javascript" src="/podiatry_pos_modifier/static/src/js/models.js"/>

                <!-- popups -->
                <script type="text/javascript" src="/podiatry_pos_modifier/static/src/js/Popups/ModifierProductPopup.js"/>
            </xpath>
        </template>
    </data>
</odoo>
