<odoo>
    <record id="action_pod_prescription" model="ir.actions.act_window">
        <field name="name">Prescriptions</field>
        <field name="res_model">pod.prescription</field>
        <field name="view_mode">kanban,tree,form,calendar,pivot,graph,activity</field>
    </record>

    <menuitem id="menu_pod_prescription" name="Prescription" action="action_pod_prescription" parent="pod_app.menu_pod" />

    <record id="action_pod_stage" model="ir.actions.act_window">
        <field name="name">Stages</field>
        <field name="res_model">pod.prescription.stage</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_pod_stage" name="Stages" action="action_pod_stage" parent="pod_app.menu_pod_configuration" />
</odoo>
