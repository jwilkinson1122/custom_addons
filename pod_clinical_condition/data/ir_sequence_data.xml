<odoo>
    <record id="seq_pod_condition" model="ir.sequence">
        <field name="name">Podiatry condition sequence</field>
        <field name="code">pod.condition</field>
        <field name="prefix">MCON</field>
        <field name="padding">6</field>
    </record>
    <record id="seq_pod_clinical_finding" model="ir.sequence">
        <field name="name">Clinical Finding sequence</field>
        <field name="code">pod.clinical.finding</field>
        <field name="prefix">CF</field>
        <field name="padding">5</field>
    </record>
    <record id="seq_pod_allergy_substance" model="ir.sequence">
        <field name="name">Clinical Allergy Substance sequence</field>
        <field name="code">pod.allergy.substance</field>
        <field name="prefix">AS</field>
        <field name="padding">5</field>
    </record>
</odoo>
