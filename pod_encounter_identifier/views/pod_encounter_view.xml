<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="pod_encounter_view_tree" model="ir.ui.view">
        <field name="name">pod.encounter.view.tree</field>
        <field name="model">pod.encounter</field>
        <field
            name="inherit_id"
            ref="pod_administration_encounter.pod_encounter_view_tree"
        />
        <field name="arch" type="xml">
            <field name="location_id" position="after">
                <field name="center_id" />
            </field>
        </field>
    </record>
    <record id="pod_encounter_view_form" model="ir.ui.view">
        <field name="name">pod.encounter.view.form</field>
        <field name="model">pod.encounter</field>
        <field
            name="inherit_id"
            ref="pod_administration_encounter.pod_encounter_view_form"
        />
        <field name="arch" type="xml">
            <field name="location_id" position="after">
                <field
                    name="center_id"
                    options="{'no_create': True, 'no_open': True}"
                />
            </field>
        </field>
    </record>
</odoo>
