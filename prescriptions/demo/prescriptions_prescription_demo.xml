<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="base.user_demo" model="res.users">
            <field name="groups_id" eval="[(3, ref('prescriptions.group_prescriptions_manager'))]"/>
        </record>

        <!-- internal -->
        <record id="prescriptions_image_city_prescription" model="prescriptions.prescription" forcecreate="0">
            <field name="name">city.jpg</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/city.jpg"/>
            <field name="folder_id" ref="prescriptions.prescriptions_internal_folder"/>
        </record>

        <record id="prescriptions_image_people_prescription" model="prescriptions.prescription" forcecreate="0">
            <field name="name">people.jpg</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/people.jpg"/>
            <field name="folder_id" ref="prescriptions.prescriptions_internal_folder"/>
        </record>


        <record id="prescriptions_data_design_brief" model="prescriptions.prescription" forcecreate="0">
            <field name="name">Interior Design Brief.pdf</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/Interior_Design_Brief.pdf"/>
            <field name="folder_id" ref="prescriptions.prescriptions_internal_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_internal_status_tc'),
                                               ref('prescriptions.prescriptions_internal_template_project')])]"/>
        </record>

        <!-- finance -->

        <record id="prescriptions_vendor_bill_inv_007" model="prescriptions.prescription" forcecreate="0">
            <field name="name">Invoice-INV_2018_0007.pdf</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/Invoice2018_0007.pdf"/>
            <field name="folder_id" ref="prescriptions.prescriptions_finance_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_finance_status_validated')])]"/>
        </record>

        <record id="prescriptions_vendor_bill_extract_azure_interior_prescription" model="prescriptions.prescription" forcecreate="0">
            <field name="name">invoice Azure Interior.pdf</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/invoice_azure_interior.pdf"/>
            <field name="folder_id" ref="prescriptions.prescriptions_finance_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_finance_status_tc')])]"/>
        </record>

        <record id="prescriptions_vendor_bill_extract_open_value_prescription" model="prescriptions.prescription" forcecreate="0">
            <field name="name">invoice OpenValue.pdf</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/invoice_openvalue.pdf"/>
            <field name="folder_id" ref="prescriptions.prescriptions_finance_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_finance_status_inbox')])]"/>
        </record>

        <record id="prescriptions_data_comercial_tenancy_agreement" model="prescriptions.prescription" forcecreate="0">
            <field name="name">Commercial-Tenancy-Agreement.pdf</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/Commercial-Tenancy-Agreement.pdf"/>
            <field name="folder_id" ref="prescriptions.prescriptions_finance_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_finance_status_inbox')])]"/>
        </record>

        <!-- marketing -->

        <record id="prescriptions_image_La_landscape_prescription" model="prescriptions.prescription" forcecreate="0">
            <field name="name">LA landscape.jpg</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/la.jpg"/>
            <field name="folder_id" ref="prescriptions.prescriptions_marketing_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_marketing_assets_images')])]"/>
        </record>

        <record id="prescriptions_attachment_sorry_netsuite_prescription" model="prescriptions.prescription" forcecreate="0">
            <field name="name">Sorry Netsuite.jpg</field>
            <field name="datas" type="base64" file="prescriptions/demo/files/sorry_netsuite.jpg"/>
            <field name="folder_id" ref="prescriptions.prescriptions_marketing_folder"/>
            <field name="tag_ids" eval="[(6,0,[ref('prescriptions.prescriptions_marketing_assets_ads')])]"/>
        </record>

    </data>
</odoo>
