<odoo>
  <data noupdate="1">
    <record id="practitioner_portal_rule" model="ir.rule">
      <field name="name">Practice Practitioner Portal Access</field>
      <field name="model_id" ref="practice_practitioner.model_practice_practitioner"/>
      <field name="domain_force">[('partner_id', '=', user.partner_id.id)]
      </field>
      <field name="groups" eval="[(4,ref('base.group_portal'))]"/>
    </record>

    <record id="bookout_portal_rule" model="ir.rule">
      <field name="name">Practice Bookout Portal Access</field>
      <field name="model_id" ref="practice_bookout.model_practice_bookout"/>
      <field name="domain_force">[('practitioner_id.partner_id', '=', user.partner_id.id)]</field>
      <field name="groups" eval="[(4,ref('base.group_portal'))]"/>
    </record>
  </data>
</odoo>
