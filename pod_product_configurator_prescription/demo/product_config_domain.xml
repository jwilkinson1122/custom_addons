<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Configuration Domain and domain Lines -->


    <record id="product_config_domain_custom_lines" model="product.config.domain">
        <field name="name">Custom Device</field>
    </record>

    <record id="product_config_domain_line_1" model="product.config.domain.line">
        <field name="domain_id" ref="product_config_domain_custom_lines" />
        <field name="attribute_id" ref="product_attribute_device" />
        <field name="condition">in</field>
        <field name="operator">and</field>
        <field name="value_ids" eval="[(6, 0, [
            ref('product_attribute_value_custom_device')])]" />
    </record>


   
    <record id="product_config_domain_superglass_lines" model="product.config.domain">
        <field name="name">Superglass Lines</field>
    </record>

    <record id="product_config_domain_line_2" model="product.config.domain.line">
        <field name="domain_id" ref="product_config_domain_superglass_lines" />
        <field name="attribute_id" ref="product_attribute_shell_foundation" />
        <field name="condition">in</field>
        <field name="operator">and</field>
        <field name="value_ids" eval="[(6, 0, [
            ref('product_attribute_value_sg_flex')
            ref('product_attribute_value_sg_everyday'),
            ref('product_attribute_value_sg_proformance')])]" />
    </record>


    <record id="product_config_domain_polypropylene_lines" model="product.config.domain">
        <field name="name">Polypropylene Lines</field>
    </record>

    <record id="product_config_domain_line_3" model="product.config.domain.line">
        <field name="domain_id" ref="product_config_domain_polypropylene_lines" />
        <field name="attribute_id" ref="product_attribute_shell_foundation" />
        <field name="condition">in</field>
        <field name="operator">and</field>
        <field name="value_ids" eval="[(6, 0, [
            ref('product_attribute_value_poly_semi_flexible'),
            ref('product_attribute_value_poly_semi_rigid'),
            ref('product_attribute_value_poly_rigid')])]" />
    </record>

    <record id="product_config_domain_ncv_lines" model="product.config.domain">
        <field name="name">NCV Lines</field>
    </record>

    <record id="product_config_domain_line_4" model="product.config.domain.line">
        <field name="domain_id" ref="product_config_domain_ncv_lines" />
        <field name="attribute_id" ref="product_attribute_shell_foundation" />
        <field name="condition">in</field>
        <field name="operator">and</field>
        <field name="value_ids" eval="[(6, 0, [
            ref('product_attribute_value_ncv_gentle'),
            ref('product_attribute_value_ncv_firm')])]" />
    </record>

    <record id="product_config_domain_pc_lines" model="product.config.domain">
        <field name="name">Prescription Comfort Lines</field>
    </record>

    <record id="product_config_domain_line_5" model="product.config.domain.line">
        <field name="domain_id" ref="product_config_domain_pc_lines" />
        <field name="attribute_id" ref="product_attribute_shell_foundation" />
        <field name="condition">in</field>
        <field name="operator">and</field>
        <field name="value_ids" eval="[(6, 0, [
            ref('product_attribute_value_pc_multi_density'),
            ref('product_attribute_value_pc_composite')])]" />
    </record>

