<odoo>

  <template id="patient_catalog" name="Patient List">
    <t t-call="web.frontend_layout">
      <t t-set="title">Patient Catalog</t>
      <div class="oe_structure">
        <div class="container">

          <h1 class="h1-patient-catalog">Patient Catalog</h1>
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Title</th>
                <th scope="col">Published</th>
                <th scope="col">Publisher</th>
              </tr>
            </thead>
            <tbody>

              <t t-foreach="patients" t-as="patient">
                <tr scope="row">
                  <td>
                    <span t-field="patient.name" />
                  </td>
                  <td>
                    <span t-field="patient.date_published" />
                  </td>
                  <td>
                    <span t-field="patient.publisher_id" />
                  </td>
                </tr>
              </t>

            </tbody>
          </table>

        </div>
      </div>
    </t>
  </template>

</odoo>
