<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="implant" model="treatment.action">
			<field name="name">Implant</field>
			<field name="action">implant</field>
			<field name="sequence">1</field>
			 <field name="replace_tooth" eval="True"/>
			 <field name="replace_racine" eval="True"/>
			<field name="img_top_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/implant-reverse2.png"/>
			<field name="img_bottom_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/implant2.png" />
			<field name="racine_bottom_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/coronne.png"/>
			<field name="racine_top_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/coronne-inv.png" />

		</record>
		
		<record id="couronnes" model="treatment.action">
			<field name="name">Couronnes</field>
			<field name="action">coronne</field>
			<field name="sequence">2</field>
			<field name="replace_tooth" eval="False"/>
			 <field name="replace_racine" eval="True"/>
			 <field name="racine_bottom_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/coronne.png"/>
			<field name="racine_top_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/coronne-inv.png"/>
		</record>

		
		<record id="extraction" model="treatment.action">
			<field name="name">Extraction</field>
			<field name="action">extraction</field>
			<field name="sequence">12</field>
			 <field name="replace_tooth" eval="True"/>
			 <field name="replace_racine" eval="False"/>
			<field name="img_top_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/empty2.png"/>
			<field name="img_bottom_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/empty2.png"/>
		</record>

		<record id="bridge" model="treatment.action">
			<field name="name">Bridge</field>
			<field name="action">bridge</field>
			<field name="sequence">3</field>
			<field name="replace_tooth" eval="True"/>
			 <field name="replace_racine" eval="True"/>
			<field name="img_top_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/bridge.png"/>
			<field name="img_bottom_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/bridge.png" />
			<field name="racine_bottom_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/coronne.png"/>
			<field name="racine_top_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/coronne-inv.png"/>
		</record>


		<record id="greffe_osseuse" model="treatment.action">
			<field name="name">Greffe Osseuse</field>
			<field name="sequence">4</field>
		</record>

		<record id="augmentation_sinus" model="treatment.action">
			<field name="name">Augmentation des sinus </field>
			<field name="sequence">5</field>
		</record>

		<record id="facette" model="treatment.action">
			<field name="name">Facette</field>
			<field name="sequence">7</field>
		</record>

		<record id="kyste" model="treatment.action">
			<field name="name">Kyste</field>
			<field name="sequence">8</field>
		</record>
		
		<record id="traitement_parodontal" model="treatment.action">
			<field name="name">Traitement Parodontal</field>
			<field name="sequence">9</field>
		</record>
		
		<record id="membrane" model="treatment.action">
			<field name="name">membrane</field>
			<field name="sequence">10</field>
		</record>





		<record id="missing" model="treatment.action">
			<field name="name">Missing</field>
			<field name="action">missing</field>

						<field name="sequence">11</field>

			 <field name="replace_tooth" eval="True"/>
			 <field name="replace_racine" eval="False"/>


			<field name="img_top_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/empty2.png"/>
			<field name="img_bottom_replace" type="base64" file="tooth_select_widget/static/src/tooth_select_field/img/empty2.png"/>


		</record>
	</data>
</odoo>