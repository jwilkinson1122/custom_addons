<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="device_type_form" model="ir.ui.view">
			<field name="name">Device Type Form View</field>
			<field name="model">device.type</field>
			<field name="arch" type="xml">
				<form string="Eye Test Config">
					<group>
						<field name="name"/>
					</group>
				</form>
			</field>
		</record>


		<record id="device_type_tree" model="ir.ui.view">
			<field name="name">Device Type Tree View</field>
			<field name="model">device.type</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
				</tree>
			</field>
		</record>


		<record id="action_config" model="ir.actions.act_window">
			<field name="name">Device Type</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">device.type</field>
			<field name="view_mode">tree,form</field>
		</record>
		<menuitem id="pod_erp_device_type" name="Device Type" parent="pod_erp_config" action="action_config" sequence="1"/>

	</data>
</odoo>
