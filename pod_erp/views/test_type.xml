<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="foot_test_type_form" model="ir.ui.view">
			<field name="name">Product Type Form View</field>
			<field name="model">foot.test.type</field>
			<field name="arch" type="xml">
				<form string="Foot Test Config">
					<group>
						<field name="name"/>
					</group>
				</form>
			</field>
		</record>


		<record id="foot_test_type_tree" model="ir.ui.view">
			<field name="name">Product Type Tree View</field>
			<field name="model">foot.test.type</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
				</tree>
			</field>
		</record>


		<record id="action_config" model="ir.actions.act_window">
			<field name="name">Product Type</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">foot.test.type</field>
			<field name="view_mode">tree,form</field>
		</record>
		<menuitem id="pod_erp_test_type" name="Shell / Foundation" parent="pod_erp_config" action="action_config" sequence="1"/>

	</data>
</odoo>
