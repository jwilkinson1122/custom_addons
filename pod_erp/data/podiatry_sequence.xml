<odoo noupdate="1">
    <!-- Sequences for podiatry prescription -->
    <record id="seq_config_session" model="ir.sequence">
        <field name="name">Configuration Session</field>
        <field name="code">product.config.session</field>
        <field name="prefix">CS</field>
        <field name="padding">4</field>
    </record>
    <record id="seq_podiatry_prescription" model="ir.sequence">
        <field name="name">Podiatry Prescription</field>
        <field name="code">podiatry.prescription</field>
        <field name="prefix">Prescription/</field>
        <field name="padding">5</field>
    </record>
</odoo>
