<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
        <record id="seq_podiatry_patient" model="ir.sequence">
            <field name="name">Patient ID</field>
            <field name="code">podiatry.patient.sequence</field>
            <field name="prefix">PID</field>
            <field name="padding">6</field>
            <!-- <field name="company_id" eval="False"/> -->
        </record>

        <record id="seq_podiatry_practice" model="ir.sequence">
            <field name="name">Practice ID</field>
            <field name="code">podiatry.practice.sequence</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
        </record>

        <record id="seq_podiatry_practitioner" model="ir.sequence">
            <field name="name">Practitioner ID</field>
            <field name="code">podiatry.practitioner.sequence</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
        </record>

        <record id="seq_podiatry_prescription" model="ir.sequence">
            <field name="name">Prescription Sequence</field>
            <field name="code">podiatry.prescription.sequence</field>
            <field name="prefix">PH-00</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>


</odoo>
