<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
        <!--sequence for practice-->
        <record id="seq_podiatry_practice" model="ir.sequence">
            <field name="name">Practice ID</field>
            <field name="code">podiatry.practice</field>
            <field name="prefix">ID</field>
            <field name="padding">6</field>
        </record>
        <!--sequence for prescription-->
        <record id="seq_podiatry_prescription" model="ir.sequence">
            <field name="name">Prescription Sequence</field>
            <field name="code">podiatry.prescription</field>
            <field name="prefix">Rx ID: %(y)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>


</odoo>
