<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
        <record id="seq_pod_prescription" model="ir.sequence">
            <field name="name">Prescription Sequence</field>
            <field name="code">pod.prescription.sequence</field>
            <field name="prefix">RX-00</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <record id="seq_pod_patient" model="ir.sequence">
        <field name="name">Patient Seq</field>
        <field name="code">pod.patient</field>
        <field name="prefix">PID</field>
        <field name="padding">5</field>
    </record>


    <!--sequence for practice-->
    <record id="seq_pod_practice" model="ir.sequence">
        <field name="name">Practice ID</field>
        <field name="code">pod.practice</field>
        <field name="prefix">ID</field>
        <field name="padding">6</field>
    </record>

    <!--sequence for doctor-->
    <record id="seq_pod_physician" model="ir.sequence">
        <field name="name">Practitioner ID</field>
        <field name="code">pod.doctor</field>
        <field name="prefix">ID</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
    </record>


</odoo>
