        <function model="prescriptions.order" name="action_confirm" eval="[[
            ref('prescriptions_order_19'),
            ref('prescriptions_order_20'),
            ref('prescriptions_order_21'),
            ref('prescriptions_order_22'),
        ]]"/>

        <!-- Change date of those prescriptions orders' delivery -->
        <function model="stock.picking" name="write">
            <value model="stock.picking" search="[('prescriptions_id', '=', obj().env.ref('pod_prescriptions_stock.prescriptions_order_19').id)]"/>
            <value eval="{'scheduled_date': (datetime.now() + relativedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')}"/>
        </function>
        <function model="stock.picking" name="write">
            <value model="stock.picking" search="[('prescriptions_id', '=', obj().env.ref('pod_prescriptions_stock.prescriptions_order_20').id)]"/>
            <value eval="{'scheduled_date': (datetime.now() + relativedelta(days=2)).strftime('%Y-%m-%d %H:%M:%S')}"/>
        </function>
        <function model="stock.picking" name="write">
            <value model="stock.picking" search="[('prescriptions_id', '=', obj().env.ref('pod_prescriptions_stock.prescriptions_order_21').id)]"/>
            <value eval="{'scheduled_date': (datetime.now() + relativedelta(days=3)).strftime('%Y-%m-%d %H:%M:%S')}"/>
        </function>


