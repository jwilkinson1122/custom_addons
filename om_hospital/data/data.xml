<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for podiatry.patient -->
        <record id="seq_hopital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">podiatry.patient</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hopital_prescription" model="ir.sequence">
            <field name="name">E-Prescription Sequence</field>
            <field name="code">podiatry.eprescription</field>
            <field name="prefix">OT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>