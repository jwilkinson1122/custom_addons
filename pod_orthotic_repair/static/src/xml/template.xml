<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="pod_orthotic_repair.orthotic_dashboard" owl="1">
        <section class="section orthotic_repair_dashboard">
            <div class="container">
                <div class="container orthotic_repair_dashboard">
                    <div class="row">
                        <div class="col-md-12 mb-4 mt32 mb16">
                            <div class="row">
                                <div class="col-12 col-sm-12 col-md-5">
                                    <h2 class="section-header">Repair Orders Details</h2>
                                </div>
                            </div>
                            <hr/>
                        </div>
                    </div>
                </div>

                <div class="row">
