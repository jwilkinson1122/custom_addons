<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="mro_quotation_tree_view" model="ir.ui.view">
            <field name="name">mro.quotation.tree.view</field>
            <field name="model">mro.quotation</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="product_id"/>
                    <field name="serial_number"/>
                    <field name="qty"/>
                    <field name="uom_id"/>
                    <field name="remarks"/>
                </tree>
            </field>
        </record>
        <record id="action_mro_quotation" model="ir.actions.act_window">
            <field name="name">MRO Quotation</field>
            <field name="res_model">mro.quotation</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>
