<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="0">
        <record id="product_category_orthotic_repair_service" model="product.category">
            <field name="parent_id" ref="product.product_category_all"/>
            <field name="name">Repair Service</field>
        </record>
        <record id="service_charge" model="product.product">
            <field name="name">Service Charges</field>
            <field name="detailed_type">service</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
        </record>

        <!--Orthotic Repair Services-->
        <record id="electrical_repair_1" model="product.product">
            <field name="name">Electrical Repair</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This includes repairing or replacing faulty electrical components such as motors,
                switches, and wiring.
            </field>
        </record>
        <record id="mechanical_repair_2" model="product.product">
            <field name="name">Mechanical Repair</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This includes repairing or replacing mechanical components such as belts, gears,
                bearings, and pulleys.
            </field>
        </record>
        <record id="hydraulic_repair_3" model="product.product">
            <field name="name">Hydraulic Repair</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This includes repairing or replacing hydraulic components such as hoses, pumps,
                and valves.
            </field>
        </record>
        <record id="pneumatic_repair_4" model="product.product">
            <field name="name">Pneumatic Repair</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This includes repairing or replacing pneumatic components such as air compressors,
                hoses, and valves.
            </field>
        </record>
        <record id="structural_repair_5" model="product.product">
            <field name="name">Structural Repair</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This includes repairing or replacing structural components such as frames, bases,
                and supports.
            </field>
        </record>
        <record id="rebuilding_and_refurbishing_6" model="product.product">
            <field name="name">Rebuilding and Refurbishing</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This includes completely disassembling and rebuilding a orthotic to restore it to
                like-new condition.
            </field>
        </record>
        <record id="preventative_maintenance_services_7" model="product.product">
            <field name="name">Preventative Maintenance Services</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This involves regularly scheduled inspections and maintenance to ensure orthotics
                are operating efficiently and to prevent unexpected breakdowns.
            </field>
        </record>
        <record id="emergency_repair_services_8" model="product.product">
            <field name="name">Emergency Repair Services</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">These are repairs that need immediate attention to minimize downtime and avoid
                production loss.
            </field>
        </record>
        <record id="predictive_maintenance_services_9" model="product.product">
            <field name="name">Predictive Maintenance Services</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This involves using data and analytics to predict potential failures before they
                occur, enabling early repairs and reducing downtime.
            </field>
        </record>
        <record id="replacement_and_upgrade_services_10" model="product.product">
            <field name="name">Replacement and Upgrade Services</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">Sometimes, orthotics may need to be replaced or upgraded to improve efficiency,
                productivity, or to comply with regulatory standards.
            </field>
        </record>
        <record id="calibration_services_11" model="product.product">
            <field name="name">Calibration Services</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">This involves ensuring orthotics are calibrated to their proper settings and in
                compliance with industry standards.
            </field>
        </record>
        <record id="welding_and_fabrication_services_12" model="product.product">
            <field name="name">Welding and Fabrication Services</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">These are repairs and maintenance services involving welding and fabrication of
                orthotic parts.
            </field>
        </record>
        <record id="cleaning_and_restoration_services_13" model="product.product">
            <field name="name">Cleaning and Restoration Services</field>
            <field name="detailed_type">service</field>
            <field name="is_repair_service">true</field>
            <field name="categ_id" ref="pod_orthotic_repair.product_category_orthotic_repair_service"/>
            <field name="description">These services involve cleaning and restoring orthotics to their original
                condition, including removing rust, grime, and other contaminants.
            </field>
        </record>

        <!--Default Project-->
        <record id="orthotic_repair_project" model="project.project">
            <field name="name">Orthotic Repair Project</field>
        </record>
    </data>
</odoo>