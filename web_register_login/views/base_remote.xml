<odoo>
    <record model="ir.ui.view" id="res_remote_form">
        <field name="name">res.remote.form (in web_register_login)</field>
        <field name="model">res.remote</field>
        <field name="inherit_id" ref="base_remote.res_remote_form" />
        <field name="arch" type="xml">
            <xpath expr="//div" position="before">
                <div class="oe_button_box" name="button_box">
                    <button
                        name="view_access_registers"
                        type="object"
                        class="oe_stat_button"
                        icon="fa-sign-in"
                        string="View Access"
                    />
                </div>
            </xpath>
        </field>
    </record>
</odoo>
