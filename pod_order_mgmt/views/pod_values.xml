<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Forefoot Values -->
    <record id="pod_forefoot_value_form" model="ir.ui.view">
        <field name="name">pod.forefoot.value.form</field>
        <field name="model">pod.forefoot.value</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="sequence"/>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="note"/>
                    </group>
                </group>
            </form>
        </field>
    </record>
    <record id="pod_forefoot_value_tree" model="ir.ui.view">
        <field name="name">pod.forefoot.value.tree</field>
        <field name="model">pod.forefoot.value</field>
        <field name="arch" type="xml">
            <tree>
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="note"/>
            </tree>
        </field>
    </record>
    <!--Action Forefoot Values -->
    <record id="forefoot_values_action" model="ir.actions.act_window">
        <field name="res_model">pod.forefoot.value</field>
        <field name="view_mode">tree,form</field>
    </record>

