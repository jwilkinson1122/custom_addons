<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="product_pod_practice_company_form_view">
        <field
            name="name"
        >product.pod.practice.company.form (in nwp_pod_pos)</field>
        <field name="model">product.pod.practice.company</field>
        <field name="arch" type="xml">
            <form>
                <header />
                <sheet>
                    <group>
                        <field name="practice_id" />
                        <field name="company_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="product_pod_practice_company_tree_view">
        <field
            name="name"
        >product.pod.practice.company.tree (in nwp_pod_pos)</field>
        <field name="model">product.pod.practice.company</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="practice_id" />
                <field name="company_id" />
            </tree>
        </field>
    </record>
</odoo>
