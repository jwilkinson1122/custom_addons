<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="product_template_custom_form_view" model="ir.ui.view">
        <field name="name">product.template.custom.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='options']" position="inside">
                <div>
                    <field name="is_tailor_product"/>
                    <label for="is_tailor_product"/>
                </div>
                <!-- <div>
                    <field name="is_delivery_product"/>
                    <label for="is_delivery_product"/>
                </div> -->

            </xpath>
        </field>
    </record>
</odoo>