<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="pod_request_group_uncheck_authorization_form_view">
        <field name="name">pod.request.group.uncheck.authorization.form (in cb_pod_authorization)</field>
        <field name="model">pod.request.group.uncheck.authorization</field>
        <field name="arch" type="xml">
            <form string="Podiatry Request Group Uncheck Authorization">
                <div>
                    You are going to Cancel this Authorization
                </div>
                <group>
                    <field name="request_group_id" invisible="1" />
                </group>
                <footer>
                    <button name="run" string="OK" class="btn-primary" type="object" />
                    <button string="Cancel" class="btn-default" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
    <record model="ir.actions.act_window" id="pod_request_group_uncheck_authorization_act_window">
        <field name="name">Podiatry Request Group Uncheck Authorization</field>
        <field name="res_model">pod.request.group.uncheck.authorization</field>
        <field name="view_mode">form</field>
        <field name="context">{}</field>
        <field name="target">new</field>
    </record>
</odoo>
